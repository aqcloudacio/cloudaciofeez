/div>\r\n        <!-- <div v-for=\"portfolio in portfolios\"\r\n             :key=\"portfolio.name\">\r\n             <p>{{ portfolio.name }}</p>\r\n        </div> -->\r\n        <hr />\r\n        <div v-for=\"investment in investments\" :key=\"investment.id\">\r\n          <div>\r\n            || {{ investment.name.name }} || ${{ investment.amount }} ||\r\n            {{ feeDescription }} ||\r\n          </div>\r\n          <DeleteObj\r\n            :objectType=\"'investment'\"\r\n            :objectToDelete=\"investment\"\r\n            :portfolioId=\"investment.portfolio\"\r\n            @delete-object=\"deleteObj\"\r\n          />\r\n          <!-- investmentdetail should be in a modal or similar -->\r\n          <InvestmentDetail\r\n            :investment=\"investment\"\r\n            :portfolioId=\"investment.portfolio\"\r\n          />\r\n        </div>\r\n        <AddInvestment @add-investment=\"addInvestment\" v-on=\"$listeners\" />\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service.js\";\r\nimport AddInvestment from \"@/components/AddInvestment.vue\";\r\nimport InvestmentDetail from \"@/components/InvestmentDetail.vue\";\r\nimport DeleteObj from \"@/components/DeleteObj.vue\";\r\n\r\nexport default {\r\n  name: \"InvestmentSummary\",\r\n  inject: [\"platformId\", \"scenarioId\"],\r\n  provide() {\r\n    return {\r\n      platformFeeId: this.platformFeeId\r\n    };\r\n  },\r\n  props: {\r\n    platformFeeId: {\r\n      type: Number,\r\n      required: true\r\n    },\r\n    feeDescription: {\r\n      type: String,\r\n      required: false\r\n    }\r\n  },\r\n  components: {\r\n    AddInvestment,\r\n    DeleteObj,\r\n    InvestmentDetail\r\n  },\r\n  data() {\r\n    return {\r\n      error: null,\r\n      portfolios: [],\r\n      investments: []\r\n    };\r\n  },\r\n  methods: {\r\n    async getPortfolioData() {\r\n      let endpoint = `/api/scenarios/${this.scenarioId}/platforms/${this.platformId}/platformfees/${this.platformFeeId}/portfolios/`;\r\n      let data = await apiService(endpoint);\r\n      this.portfolios.push(...data.results);\r\n      this.getInvestmentData();\r\n    },\r\n    getInvestmentData() {\r\n      if (this.portfolios) {\r\n        for (let i = 0; i < this.portfolios.length; i++) {\r\n          let endpoint = `/api/scenarios/${this.scenarioId}/platforms/${this.platformId}/platformfees/${this.platformFeeId}/portfolios/${this.portfolios[i].id}/investments/`;\r\n          apiService(endpoint).then(data => {\r\n            this.investments.push(...data.results);\r\n          });\r\n        }\r\n      }\r\n    },\r\n    addInvestment(investment) {\r\n      this.investments.push(investment);\r\n    },\r\n    deleteObj(objectToDelete) {\r\n      this.$delete(this.investments, this.investments.indexOf(objectToDelete));\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getPortfolioData();\r\n  }\r\n};\r\n</script>\r\n"]}]}