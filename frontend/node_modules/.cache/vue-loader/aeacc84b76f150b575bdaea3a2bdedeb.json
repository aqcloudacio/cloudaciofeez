<p>{{ investment.buy_spread | toPercentage }} / {{ investment.sell_spread | toPercentage }}</p>\r\n          <p>{{ investment.APIR_code }}{{ investment.ASX_code }}{{ investment.other_code }}</p>\r\n          <p>{{ investment.investment_class.name }}</p>\r\n          <p v-if=\"investment.cash\">Nominated cash account</p>\r\n          <p v-if=\"investment.TD\">Term deposit</p>\r\n\r\n          <div v-for=\"aa in aaList\"\r\n               :key=\"aa.id\">\r\n               <p>{{ aa.name.name }} {{ aa.percentage | toPercentage }}</p>\r\n          </div>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service.js\";\r\n\r\nexport default {\r\n  name: \"InvestmentSummary\",\r\n  inject: ['platformId',\r\n           'scenarioId',\r\n           'platformFeeId'],\r\n  props: {\r\n    investment: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n    portfolioId: {\r\n      type: Number,\r\n      required: true\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      error: null,\r\n      aaList: [],\r\n    }\r\n  },\r\n  methods: {\r\n    getAAData() {\r\n      let endpoint = `/api/scenarios/${this.scenarioId}/platforms/${this.platformId}/platformfees/${this.platformFeeId}/portfolios/${this.portfolioId}/investments/${this.investment.id}/aa/`;\r\n      apiService(endpoint)\r\n        .then(data => {\r\n          this.aaList.push(...data.results);\r\n        })\r\n    }\r\n  },\r\n  created() {\r\n    this.getAAData();\r\n  },\r\n}\r\n\r\n</script>\r\n"]}]}