ce && this.activeUser.active_practice\"\r\n        >\r\n          <v-list-item-title>Share with {{this.activeUser.active_practice_name}}</v-list-item-title>\r\n        </v-list-item>\r\n        <v-list-item\r\n          @click='dialog=true'\r\n        >\r\n          <v-list-item-title>Share with someone else</v-list-item-title>\r\n        </v-list-item>\r\n        <v-list-item\r\n          @click='dialog=true'\r\n          v-if=\"!_.isEmpty(scenario.allowed_users)\"\r\n        >\r\n          <v-list-item-title>Unshare with someone else</v-list-item-title>\r\n        </v-list-item>\r\n      </v-list>\r\n    </v-menu>\r\n    <v-dialog\r\n      max-width=\"500px\"\r\n      v-model=\"dialog\">\r\n      <v-card>\r\n        <v-card-title class=\"headline grey lighten-2 pr-3 pb-4\">\r\n          Sharing Settings\r\n          <v-spacer></v-spacer>\r\n          <v-btn\r\n            color=\"secondary\"\r\n            text\r\n            @click=\"dialog = false\"\r\n          >X</v-btn>\r\n        </v-card-title>\r\n\r\n        <v-container class=\"pa-3\">\r\n          <v-form\r\n            ref=\"form\">\r\n            <v-text-field\r\n              :placeholder=\"`Enter email address to share ${scenario.client} with`\"\r\n              label=\"Email address\"\r\n              v-model=\"newShareEmail\"\r\n              dense outlined\r\n              :error-messages=\"error ? error.message : null\"\r\n              :rules=\"$ruleEmail\"\r\n              hide-details=\"auto\"\r\n              @click=\"error = null\"\r\n            ></v-text-field>\r\n            <v-card-actions>\r\n              <v-spacer />\r\n              <v-btn text :color=\"'primary'\"\r\n              @click=\"addAllowedUser()\">\r\n                Share\r\n              </v-btn>\r\n            </v-card-actions>\r\n            <template v-if=\"!_.isEmpty(scenario.allowed_users)\">\r\n              <v-divider  />\r\n              <v-card-title>\r\n                Shared Users\r\n              </v-card-title>\r\n              <v-card\r\n                v-for=\"allowed_user in scenario.allowed_users\"\r\n                :key=\"allowed_user.id\"\r\n                class=\"mx-4 \"\r\n                >\r\n                <v-row dense>\r\n                  <v-col dense>\r\n                    <v-card-text class=\"py-2 px-3\">\r\n                      {{ allowed_user.name ? allowed_user.name : allowed_user.email }}\r\n                    </v-card-text>\r\n                  </v-col>\r\n                  <v-col>\r\n                    <v-card-actions>\r\n                      <v-spacer />\r\n                      <v-tooltip right>\r\n                        <template v-slot:activator=\"{ on }\">\r\n                          <v-btn icon small v-on=\"on\">\r\n                            <v-icon\r\n                              :color=\"'danger'\"\r\n                              @click=\"removeAllowedUser(allowed_user)\">\r\n                              mdi-account-remove-outline\r\n                            </v-icon>\r\n                          </v-btn>\r\n                        </template>\r\n                        <span>Unshare</span>\r\n                      </v-tooltip>\r\n                    </v-card-actions>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-card>\r\n            </template>\r\n          </v-form>\r\n        </v-container>\r\n      </v-card>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service.js\";\r\nimport { ruleEmail } from \"@/components/mixins/Rules.js\"\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"ShareScenario\",\r\n  mixins: [ruleEmail],\r\n  props: {\r\n    scenario: {\r\n      type: Object,\r\n      required: true,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      error: null,\r\n      dialog: false,\r\n      newShareEmail: null,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"activeUser\"]),\r\n  },\r\n  methods: {\r\n    shareWithPractice() {\r\n      let endpoint = `/api/scenarios/${this.scenario.id}/`;\r\n      apiService(endpoint, \"PATCH\", {\r\n        practice: this.activeUser.active_practice\r\n        }).then(data => {\r\n          this.$emit('update-scenarios', data);\r\n          this.scenario.practice = data.practice\r\n      });\r\n    },\r\n    addAllowedUser() {\r\n      if (this.newShareEmail) {\r\n        let newAllowedUser = new Object();\r\n        newAllowedUser.email = this.newShareEmail;\r\n        let allowed_users = this.scenario.allowed_users.push(newAllowedUser);\r\n        let endpoint = `/api/scenarios/${this.scenario.id}/`;\r\n        apiService(endpoint, \"PATCH\", {\r\n          allowed_users: allowed_users\r\n          }).then(data => {\r\n            this.$emit('update-scenarios', data);\r\n            this.scenario.allowed_users = data.allowed_users;\r\n          }).catch(error => {\r\n            this.scenario.allowed_users.pop()\r\n            this.error = error;\r\n          })\r\n        }\r\n    },\r\n    removeAllowedUser(allowed_user) {\r\n      let newAllowedUsers = this.scenario.allowed_users.filter(function(el) { return el.id != allowed_user.id; }); ;\r\n      let endpoint = `/api/scenarios/${this.scenario.id}/`;\r\n      apiService(endpoint, \"PATCH\", {\r\n        allowed_users: newAllowedUsers\r\n        }).then(data => {\r\n          this.$emit('update-scenarios', data);\r\n          this.scenario.allowed_users = data.allowed_users;\r\n      });\r\n    },\r\n  }\r\n};\r\n</script>\r\n"]}]}