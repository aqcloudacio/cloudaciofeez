s: {\r\n    requestUser: {},\r\n  },\r\n  methods: {\r\n    onSubmit() {\r\n      // Tell the REST API to create a new answer for this question based on the user input, then update some data properties\r\n      if (this.newScenario) {\r\n        let endpoint = `/api/scenarios/`;\r\n        apiService(endpoint, \"POST\", { client: this.newScenario,\r\n                                       user: this.requestUser})\r\n        .then(data => {\r\n          this.$emit(\"add-scenario\", data);\r\n          this.newScenario = null;\r\n        })\r\n        if (this.error) {\r\n          this.error = null;\r\n        }\r\n      } else {\r\n        this.error = \"You must enter a name for the scenario\";\r\n      }\r\n    },\r\n  },\r\n}\r\n\r\n</script>\r\n"]}]}