\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service.js\";\r\nexport default {\r\n  name: \"AddPlatform\",\r\n  inject: ['scenarioId'],\r\n  data() {\r\n    return {\r\n      newPlatform: {},\r\n      platformList: [],\r\n      error: null\r\n    }\r\n  },\r\n  methods: {\r\n    getPlatformList() {\r\n      let endpoint = `/api/platformnames/`;\r\n      apiService(endpoint, \"GET\")\r\n        .then(data => {\r\n          this.platformList.push(...data.results)\r\n        })\r\n    },\r\n    onSubmit() {\r\n      // Tell the REST API to create a new answer for this question based on the user input, then update some data properties\r\n      if (this.newPlatform) {\r\n        let endpoint = `/api/scenarios/${this.scenarioId}/platforms/`;\r\n        apiService(endpoint, \"POST\", { name_id: this.newPlatform.id,\r\n                                       scenario: this.scenarioId})\r\n        .then(data => {\r\n          this.$emit(\"add-platform\", data);\r\n          this.newPlatform = null;\r\n        })\r\n        if (this.error) {\r\n          this.error = null;\r\n        }\r\n      } else {\r\n        this.error = \"You must select a platform\";\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getPlatformList();\r\n  }\r\n}\r\n\r\n</script>\r\n"]}]}