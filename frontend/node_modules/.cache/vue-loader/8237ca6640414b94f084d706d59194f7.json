 outlined\r\n            hide-details/>\r\n          <v-select\r\n            :value=\"scenario.risk_profile\"\r\n            @input=\"scenario.risk_profile_id = $event\"\r\n            :items=\"riskProfiles\"\r\n            item-value=\"id\"\r\n            item-text=\"name\"\r\n            dense\r\n            outlined\r\n            hide-details/>\r\n          <v-btn\r\n            type=\"submit\"\r\n            color=\"primary\"\r\n            @click=\"onSubmit(scenario)\">\r\n              Save\r\n          </v-btn>\r\n        </v-card>\r\n        <v-card\r\n          v-else\r\n          height=\"100%\"\r\n          :to=\"{ name: 'scenario', params: { id: scenario.id } }\">\r\n          <v-card-title>\r\n            {{ scenario.client }} <br>\r\n            {{ scenario.scenario_total | toCurrency }} <br>\r\n            {{ scenario.risk_profile == null ? \"\" : scenario.risk_profile_name }}\r\n          </v-card-title>\r\n        </v-card>\r\n\r\n      </v-col>\r\n      <v-col\r\n        class=\"px-0\">\r\n        <v-card\r\n          height=\"100%\"\r\n          class=\"pa-4\">\r\n          <v-chip\r\n            v-for=\"platform in scenario.platforms\"\r\n            :key=\"platform[0]\"\r\n            class=\"ma-1\"\r\n            color=\"success\"\r\n            outlined\r\n            :to=\"{ name: 'platform', params: { scenarioId: scenario.id, id: platform[0] } }\">\r\n            {{ platform[1] }}\r\n          </v-chip>\r\n        </v-card>\r\n      </v-col>\r\n      <v-col\r\n        class=\"px-0\">\r\n        <v-card\r\n          height=\"100%\">\r\n          <v-card-actions>\r\n            <v-btn\r\n              color=\"blue\"\r\n              :to=\"{ name: 'output', params: { scenarioId: scenario.id } }\">\r\n              <v-icon color=\"white\">\r\n                mdi-file-document-outline\r\n              </v-icon>\r\n            </v-btn>\r\n            <v-btn color=\"green\">\r\n              <v-icon color=\"white\">\r\n                mdi-share-variant\r\n              </v-icon>\r\n            </v-btn>\r\n            <v-btn\r\n              color=\"orange\"\r\n              @click=\"doEdit(scenario.id)\">\r\n              <v-icon color=\"white\">\r\n                mdi-pencil\r\n              </v-icon>\r\n            </v-btn>\r\n            <DeleteObj\r\n              :objectToDelete=\"scenario\"\r\n              :objectType=\"'scenario'\"\r\n              @delete-object=\"deleteObj\"\r\n            />\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport DeleteObj from \"@/components/DeleteObj.vue\";\r\nimport { apiService } from \"@/common/api.service.js\";\r\n\r\n\r\nexport default {\r\n  name: \"ScenarioSummary\",\r\n  props: {\r\n    scenarios: {\r\n      type: Array,\r\n      required: true\r\n    },\r\n    riskProfiles: {\r\n      type: Array,\r\n      required: true,\r\n    }\r\n  },\r\n  components: {\r\n    DeleteObj\r\n  },\r\n  data() {\r\n    return {\r\n      error: null,\r\n      href: null,\r\n      route: null,\r\n      navigate: null,\r\n      edit: [],\r\n    };\r\n  },\r\n  methods: {\r\n    deleteObj(objectToDelete) {\r\n      this.$delete(this.scenarios, this.scenarios.indexOf(objectToDelete));\r\n    },\r\n    doEdit(id){\r\n      if (this.edit.includes(id)) {\r\n        this.edit.splice(this.edit.indexOf(id));\r\n      } else {\r\n      this.edit.push(id);\r\n      }\r\n    },\r\n    updateScenarios(scenario) {\r\n      let idx = (this.scenarios.findIndex(i => i.id === scenario.id));\r\n      this.scenarios[idx] = scenario;\r\n    },\r\n    onSubmit(scenario){\r\n      let endpoint = `/api/scenarios/${scenario.id}/`;\r\n      apiService(endpoint, \"PATCH\", scenario\r\n        ).then(data => {\r\n        this.doEdit(scenario.id);\r\n        this.updateScenarios(data);\r\n        this.$emit(\"edit-scenario\",scenario);\r\n\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}