   </select>\r\n        <input type=\"number\" v-model=\"investmentAmount\" />\r\n      </div>\r\n      {{ error }}\r\n      <div class=\"card-footer px-3\">\r\n        <button type=\"submit\" class=\"btn btn-sm btn-success\">Add</button>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service.js\";\r\nexport default {\r\n  name: \"AddInvestment\",\r\n  inject: [\"platformId\", \"scenarioId\", \"platformFeeId\"],\r\n  data() {\r\n    return {\r\n      newInvestment: {},\r\n      investmentAmount: null,\r\n      investmentList: [],\r\n      portfolioId: null,\r\n      error: null\r\n    };\r\n  },\r\n  methods: {\r\n    getInvestmentList() {\r\n      let endpoint = `/api/investmentname/`;\r\n      apiService(endpoint, \"GET\").then(data => {\r\n        this.investmentList.push(...data.results);\r\n      });\r\n    },\r\n    getPortfolioId() {\r\n      let endpoint = `/api/scenarios/${this.scenarioId}/platforms/${this.platformId}/platformfees/${this.platformFeeId}/portfolios/`;\r\n      apiService(endpoint, \"GET\").then(data => {\r\n        this.portfolioId = data.results[0].id;\r\n      });\r\n    },\r\n    onSubmit() {\r\n      if (this.newInvestment) {\r\n        let endpoint = `/api/scenarios/${this.scenarioId}/platforms/${this.platformId}/platformfees/${this.platformFeeId}/portfolios/${this.portfolioId}/investments/`;\r\n        apiService(endpoint, \"POST\", {\r\n          name_id: this.newInvestment.id,\r\n          investment_class_id: \"\",\r\n          portfolio: this.portfolioId,\r\n          amount: this.investmentAmount\r\n        }).then(data => {\r\n          this.$emit(\"add-investment\", data);\r\n          this.newInvestment = null;\r\n          this.investmentAmount = null;\r\n        });\r\n        if (this.error) {\r\n          this.error = null;\r\n        }\r\n      } else {\r\n        this.error = \"You must select an investment and enter an amount\";\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.getInvestmentList();\r\n    this.getPortfolioId();\r\n  }\r\n};\r\n</script>\r\n"]}]}