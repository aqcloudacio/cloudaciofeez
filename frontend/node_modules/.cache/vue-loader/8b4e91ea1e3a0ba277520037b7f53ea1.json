ard-text v-if=\"!editProfile\">\r\n                <span v-if=\"activeUser.name\">\r\n                  {{activeUser.name}} <br>\r\n                </span>\r\n              {{activeUser.email}} <br>\r\n                <span v-if=\"activeUser.role\">\r\n                  {{activeUser.role}} <br>\r\n                </span>\r\n              </v-card-text>\r\n              <v-text-field\r\n                dense\r\n                class=\"smallFont px-4 py-2 \"\r\n                hide-details=\"auto\"\r\n                v-if=\"editProfile\"\r\n                v-model=\"profileName\">\r\n              </v-text-field>\r\n              <v-text-field\r\n                dense\r\n                class=\"smallFont  px-4 pb-2 pt-0\"\r\n                hide-details=\"auto\"\r\n                v-if=\"editProfile\"\r\n                v-model=\"profileEmail\">\r\n              </v-text-field>\r\n              <v-combobox\r\n                class=\"smallFont  px-4 pb-4 pt-0\"\r\n                dense\r\n                v-if=\"editProfile\"\r\n                v-model=\"profileRole\"\r\n                :items=\"roleList\"\r\n                ref=\"combobox\"\r\n                @keyup.space.prevent\r\n                @click.stop\r\n                hide-details=\"auto\"\r\n                label=\"Your Role\"\r\n                @keyup.enter=\"blurComboBox()\">\r\n              <template v-slot:no-data>\r\n                <v-list-item>\r\n                  <v-list-item-content>\r\n                    <v-list-item-title>\r\n                      Press <kbd>Enter</kbd> to create a custom role.\r\n                    </v-list-item-title>\r\n                  </v-list-item-content>\r\n                </v-list-item>\r\n              </template>\r\n            </v-combobox>\r\n        </v-col>\r\n        <v-col :cols=\"1\" >\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn icon>\r\n              <v-icon\r\n                v-if=\"!editProfile\"\r\n                color=\"green\" icon\r\n                @click=\"editProfile = !editProfile\">\r\n                mdi-pencil-circle\r\n              </v-icon>\r\n              <v-icon\r\n                v-else\r\n                color=\"green\" icon\r\n                @click=\"updateUser()\">\r\n                mdi-check-circle\r\n              </v-icon>\r\n            </v-btn>\r\n          </v-card-actions>\r\n        </v-col>\r\n      </v-row>\r\n    </v-card>\r\n  </v-col>\r\n  <v-col :cols=\"6\">\r\n    <v-card>\r\n      <v-form ref=\"AFSLForm\">\r\n\r\n      <v-row>\r\n        <v-col class=\"py-0\" :cols=\"10\">\r\n          <v-card-text\r\n            v-if=\"!editAFSL && !profileHasAFSL\"\r\n            @click=\"editAFSL = !editAFSL\">\r\n             Add button\r\n          </v-card-text>\r\n           <v-card-text v-else-if=\"!editAFSL && profileHasAFSL\">\r\n             <span v-if=\"activeUser.AFSL_approved\">\r\n               AFSL: {{activeUser.AFSL.name}} <br>\r\n             </span>\r\n             <span v-else>\r\n               AFSL Linking Pending Approval <br>\r\n             </span>\r\n             <span v-if=\"activeUser.AFSL\">\r\n               AFSL Number: {{activeUser.AFSL.AFSL_number}} <br>\r\n             </span>\r\n             <span v-if=\"activeUser.linked_ARN\">\r\n               Linked ARN: {{activeUser.linked_ARN}} <br>\r\n             </span>\r\n           </v-card-text>\r\n           <template v-else>\r\n             <v-text-field\r\n              dense\r\n               class=\"smallFont px-4 pt-3 ma-0\"\r\n               hide-details=\"auto\"\r\n               v-if=\"editAFSL\"\r\n               v-model=\"profileAFSL\"\r\n               required\r\n               :rules=\"[v => !!v || 'You must enter an AFSL Number']\"\r\n               label=\"Enter AFSL Number\">\r\n             </v-text-field>\r\n             <v-text-field\r\n              dense\r\n               class=\"smallFont  px-4 pb-4 pt-3 ma-0\"\r\n               v-if=\"editAFSL\"\r\n               v-model=\"profileLinkedARN\"\r\n               hint=\"If you are not an adviser, enter the ARN of the principal adviser\"\r\n               persistent-hint\r\n               required\r\n               hide-details=\"auto\"\r\n               :rules=\"[v => !!v || 'You must enter an ARN. If you are not an adviser, enter the ARN of the principal adviser.']\"\r\n               label=\"Authored Rep Number\">\r\n             </v-text-field>\r\n           </template>\r\n            </v-col>\r\n            <v-col :cols=\"1\" >\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn icon>\r\n                  <v-icon\r\n                    v-if=\"!editAFSL\"\r\n                    color=\"green\" icon\r\n                    @click=\"editAFSL = !editAFSL\">\r\n                    mdi-pencil-circle\r\n                  </v-icon>\r\n                  <v-icon\r\n                    v-else\r\n                    color=\"green\" icon\r\n                    @click=\"updateAFSL()\">\r\n                    mdi-check-circle\r\n                  </v-icon>\r\n                </v-btn>\r\n              </v-card-actions>\r\n            </v-col>\r\n          </v-row>\r\n        </v-form>\r\n\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n    <v-row >\r\n      <v-col :cols=\"3\">\r\n        <v-card\r\n          height=\"100%\"\r\n          min-height=\"100px\"\r\n          class=\"text-center\"\r\n          @click=\"active='themes'\">\r\n          <v-card-title class=\"justify-center\">\r\n            Theme\r\n          </v-card-title>\r\n          <v-chip\r\n              v-if=\"activeUser.active_theme_name\"\r\n               label\r\n              color=\"teal lighten-4\"\r\n              >\r\n              {{activeUser.active_theme_name}}\r\n            </v-chip>\r\n        </v-card>\r\n      </v-col>\r\n      <v-col :cols=\"3\">\r\n        <v-card\r\n          height=\"100%\"\r\n          min-height=\"100px\"\r\n          :class=\"activeUser.active_practice_name ? 'text-center' : 'd-flex justify-center'\"\r\n          @click=\"active='practices'\">\r\n\r\n          <v-card-title class=\"justify-center\">\r\n            Practice\r\n          </v-card-title>\r\n          <v-chip\r\n              v-if=\"activeUser.active_practice_name\"\r\n              label\r\n              color=\"teal lighten-4\"\r\n              >\r\n              {{activeUser.active_practice_name}}\r\n            </v-chip>\r\n\r\n        </v-card>\r\n      </v-col>\r\n      <v-col :cols=\"3\">\r\n        <v-card\r\n          height=\"100%\"\r\n          min-height=\"100px\"\r\n          :class=\"activeRPName ? 'text-center' : 'd-flex justify-center'\"\r\n          @click=\"active='riskProfile'\">\r\n          <v-card-title class=\"justify-center\">\r\n            Risk Profiler\r\n          </v-card-title>\r\n          <v-chip\r\n            v-if=\"activeRPName\"\r\n            class=\"mb-4\" label\r\n            color=\"teal lighten-4\">\r\n            {{activeRPName}}\r\n          </v-chip>\r\n        </v-card>\r\n      </v-col>\r\n      <v-col :cols=\"3\">\r\n        <v-card\r\n          height=\"100%\"\r\n          min-height=\"100px\"\r\n          class=\" d-flex justify-center\"\r\n          @click=\"active='portfolio'\">\r\n          <v-card-title>\r\n            Model Portfolios\r\n          </v-card-title>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n  <v-card>\r\n    <v-container>\r\n      <ThemeTable\r\n        v-if=\"active == 'themes'\"\r\n        />\r\n      <PortfolioTable\r\n        v-if=\"active == 'portfolio'\"\r\n        />\r\n      <PracticeTable\r\n        v-if=\"active == 'practices'\"\r\n        :riskProfileGroups=\"riskProfileGroups\"\r\n        />\r\n      <RiskProfileTable\r\n        v-if=\"active == 'riskProfile'\"\r\n        :riskProfileGroups=\"riskProfileGroups\"\r\n        @add-rpgroup=\"addRPGroup($event)\"\r\n        @update-rpgroup=\"updateRPGroup($event)\"\r\n        @delete-rpgroup=\"removeRPGroup($event)\"\r\n       />\r\n    </v-container>\r\n  </v-card >\r\n</v-container>\r\n\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service\";\r\nimport RiskProfileTable from \"@/components/RiskProfileTable.vue\";\r\nimport ThemeTable from \"@/components/ThemeTable.vue\";\r\nimport PortfolioTable from \"@/components/PortfolioTable.vue\";\r\nimport PracticeTable from \"@/components/PracticeTable.vue\";\r\nimport { mapGetters } from \"vuex\";\r\nimport {\r\n  SET_ACTIVE_USER_PROP,\r\n} from \"@/store/mutations.type\";\r\nimport {\r\n  USER_UPDATE\r\n} from \"@/store/actions.type\";\r\n\r\nexport default {\r\n  name: \"profile\",\r\n  props: {\r\n    shortcut: {\r\n      type: String,\r\n      required: false\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      riskProfileGroups: null,\r\n      active: 'themes',\r\n      editProfile: false,\r\n      editAFSL: false,\r\n      roleList: [\r\n        \"Adviser\",\r\n        \"Practice Manager\",\r\n        \"Paraplanner\",\r\n        \"Administrator\",\r\n        \"Support\"\r\n      ]\r\n    };\r\n  },\r\n  components: {\r\n    RiskProfileTable,\r\n    PortfolioTable,\r\n    PracticeTable,\r\n    ThemeTable,\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"activeUser\"]),\r\n    profileName: {\r\n      get() {\r\n        return this.activeUser.name\r\n      },\r\n      set(value){\r\n        let first_name = value.substr(0,value.indexOf(' '));\r\n        let last_name = value.substr(value.indexOf(' ')+1);\r\n        this.$store.commit(SET_ACTIVE_USER_PROP, {\r\n          prop: \"name\",\r\n          value: value\r\n        });\r\n        this.$store.commit(SET_ACTIVE_USER_PROP, {\r\n          prop: \"first_name\",\r\n          value: first_name\r\n        });\r\n        this.$store.commit(SET_ACTIVE_USER_PROP, {\r\n          prop: \"last_name\",\r\n          value: last_name\r\n        });\r\n      }\r\n    },\r\n    profileEmail: {\r\n      get() {\r\n        return this.activeUser.email\r\n      },\r\n      set(value){\r\n        this.$store.commit(SET_ACTIVE_USER_PROP, {\r\n          prop: \"email\",\r\n          value: value\r\n        });\r\n      }\r\n    },\r\n    profileRole: {\r\n      get() {\r\n        return this.activeUser.role\r\n      },\r\n      set(value){\r\n        this.$store.commit(SET_ACTIVE_USER_PROP, {\r\n          prop: \"role\",\r\n          value: value\r\n        });\r\n      }\r\n    },\r\n    profileHasAFSL() {\r\n      return this.activeUser.linked_ARN > 0 && this.activeUser.AFSL\r\n    },\r\n    profileAFSL: {\r\n      get() {\r\n        if (this.profileHasAFSL) {\r\n          return this.activeUser.AFSL.AFSL_number\r\n        } else {\r\n          return null\r\n        }\r\n      },\r\n      set(value){\r\n        this.$store.commit(SET_ACTIVE_USER_PROP, {\r\n          prop: \"AFSL.AFSL_number\",\r\n          value: value\r\n        });\r\n      }\r\n    },\r\n    profileLinkedARN: {\r\n      get() {\r\n        return this.activeUser.linked_ARN\r\n      },\r\n      set(value){\r\n        this.$store.commit(SET_ACTIVE_USER_PROP, {\r\n          prop: \"linked_ARN\",\r\n          value: value\r\n        });\r\n      }\r\n    },\r\n    practices() {\r\n      if (this.activeUser.practice_names) {\r\n        return this.activeUser.practice_names\r\n      } else {\r\n        return null\r\n      }\r\n    },\r\n    activeRPName() {\r\n      if (this.riskProfileGroups && this.activeUser) {\r\n        const result = this.riskProfileGroups.find(\r\n            x => x.id === this.activeUser.active_rp)\r\n            if (result) {\r\n              //for some reason splicing the array causes this to temporarily\r\n              // not find the riskProfileGroup array. This If statement avoids\r\n              // a warning message.\r\n              return result.name\r\n            } else {\r\n              return null\r\n            }\r\n      } else {\r\n        return null\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    blurComboBox() {\r\n      // Blurs the combobox when users creates custom role.\r\n      this.$refs.combobox.blur();\r\n      // this.$nextTick(() => {\r\n      //   this.profileRole();\r\n      // })\r\n    },\r\n    updateAFSL() {\r\n      this.editAFSL = !this.editAFSL;\r\n      this.$store.dispatch(USER_UPDATE);\r\n    },\r\n    updateUser() {\r\n      this.editProfile = !this.editProfile;\r\n      this.$store.dispatch(USER_UPDATE);\r\n    },\r\n    addRPGroup() {\r\n      this.getRPGroups();\r\n    },\r\n    removeRPGroup(data) {\r\n      const i = this.riskProfileGroups.findIndex(item => item.id === data.id);\r\n      this.riskProfileGroups.splice(i);\r\n    },\r\n    updateRPGroup(data) {\r\n      const i = this.riskProfileGroups.findIndex(item => item.id === data.id);\r\n      this.riskProfileGroups.splice(i,0,data);\r\n    },\r\n    getRPGroups() {\r\n      let endpoint = `/api/riskprofilegroups/`;\r\n      apiService(endpoint).then(data => {\r\n        this.riskProfileGroups = data.results;\r\n      });\r\n    },\r\n    checkShortcut() {\r\n      if (this.shortcut) {\r\n        this.active = this.shortcut;\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getRPGroups();\r\n    this.checkShortcut();\r\n  }\r\n};\r\n</script>\r\n<style media=\"screen\">\r\n.smallFont {\r\n  font-size: 0.9em;\r\n}\r\n.smallFont label {\r\n  font-size: 1em;\r\n}\r\n</style>\r\n"]}]}