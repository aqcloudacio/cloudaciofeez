\n              </v-card-title>\r\n              <v-card-subtitle class=\"headline\">\r\n                <template v-if=\"investment.investment_class\">\r\n                  {{ investment.investment_class.name }}\r\n                </template>\r\n                {{ investment.APIR_code }}{{ investment.ASX_code\r\n                }}{{ investment.other_code }}\r\n              </v-card-subtitle>\r\n              <v-card-text>\r\n                <v-text-field\r\n                  v-model=\"investment.amount\"\r\n                  :value=\"investment.amount | toCurrency\"\r\n                  label=\"Amount\"\r\n                  clearable outlined dense\r\n                  >\r\n\r\n              </v-text-field>\r\n\r\n\r\n                {{ +investment.investment_fee | toPercentage }}<br>\r\n                  {{ +investment.buy_spread | toPercentage }} /\r\n                  {{ +investment.sell_spread | toPercentage }}<br>\r\n                 <p v-if=\"investment.cash\">Nominated cash account</p>\r\n                <p v-if=\"investment.TD\">Term deposit</p>\r\n              </v-card-text>\r\n            </v-card>\r\n          </v-col>\r\n          <v-col>\r\n            <v-card>\r\n              <v-card-title>Asset Allocation</v-card-title>\r\n              <v-card-text>\r\n                  <span v-for=\"aa in aaList\" :key=\"aa.id\">\r\n                    {{ aa.name.name }} {{ +aa.percentage | toPercentage }} <br>\r\n                  </span>\r\n              </v-card-text>\r\n            </v-card>\r\n          </v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </v-form>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service.js\";\r\n\r\nexport default {\r\n  name: \"InvestmentDetail\",\r\n  inject: [\"platformId\", \"scenarioId\", \"platformFeeId\"],\r\n  props: {\r\n    investment: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n    portfolioId: {\r\n      type: Number,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      error: null,\r\n      dialog: false,\r\n      aaList: []\r\n    };\r\n  },\r\n  watch: {\r\n    investment.name() {\r\n      this.investment.name = this.$options.filters.toCurrency(this.investment.name);\r\n    }\r\n  },\r\n  methods: {\r\n    getAAData() {\r\n      let endpoint = `/api/scenarios/${this.scenarioId}/platforms/${this.platformId}/platformfees/${this.platformFeeId}/portfolios/${this.portfolioId}/investments/${this.investment.id}/aa/`;\r\n      apiService(endpoint).then(data => {\r\n        this.aaList.push(...data.results);\r\n      });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getAAData();\r\n  }\r\n};\r\n</script>\r\n"]}]}