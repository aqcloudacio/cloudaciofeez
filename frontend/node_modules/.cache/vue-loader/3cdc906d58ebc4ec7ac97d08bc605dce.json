scenarioId:this.id } }\"\r\n              color=\"secondary\"\r\n              justify=\"right\">\r\n                  Results\r\n        </v-btn>\r\n      <v-row>\r\n        <v-col>\r\n          <v-row>\r\n            <v-col>\r\n              HEALTHCHECK\r\n            </v-col>\r\n          </v-row>\r\n          <v-row>\r\n              <v-col>\r\n                Fees\r\n              </v-col>\r\n          </v-row>\r\n        </v-col>\r\n        <v-col>\r\n          AA CHART\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n    <v-container\r\n      class=\"grey lighten-2\">\r\n      <v-row>\r\n        <v-col\r\n          v-for=\"status in statuses\"\r\n          :key=\"status.index\">\r\n          <v-card>\r\n            <v-card-title\r\n              class=\"justify-center\">\r\n            {{ status }}\r\n            </v-card-title>\r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row>\r\n        <v-col\r\n          v-for=\"status in statuses\"\r\n          :key=\"status.index\">\r\n          <AddPlatform\r\n            :status=\"status\"\r\n            @add-platform=\"addPlatform\" />\r\n        </v-col>\r\n      </v-row>\r\n      <v-row>\r\n        <v-col\r\n          v-for=\"status in statuses\"\r\n          :key=\"status.index\">\r\n          <v-row>\r\n            <draggable\r\n              :v-model=\"filterByStatus(status)\"\r\n              :group=\"statusGroups\"\r\n              @start=\"drag=true\"\r\n              @end=\"drag=false\"\r\n              @change=\"updateStatus\">\r\n            <v-col\r\n              v-for=\"platform in filterByStatus(status)\"\r\n              :key=\"platform.id\">\r\n\r\n                    <PlatformSummary\r\n\r\n                      :scenario=\"scenario\"\r\n                      :platform=\"platform\"\r\n                      @delete-object=\"deleteObj\"\r\n                    />\r\n            </v-col>\r\n          </draggable>\r\n          </v-row>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n\r\n    <AddCustomPlatform />\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service.js\";\r\nimport AddPlatform from \"@/components/AddPlatform.vue\";\r\nimport AddCustomPlatform from \"@/components/AddCustomPlatform.vue\";\r\nimport PlatformSummary from \"@/components/PlatformSummary.vue\";\r\nimport draggable from 'vuedraggable'\r\n\r\nexport default {\r\n  name: \"Scenario\",\r\n  provide() {\r\n    return {\r\n      scenarioId: this.id\r\n    };\r\n  },\r\n  props: {\r\n    id: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  components: {\r\n    AddPlatform,\r\n    PlatformSummary,\r\n    AddCustomPlatform,\r\n    draggable,\r\n  },\r\n  data() {\r\n    return {\r\n      scenario: {},\r\n      platforms: [],\r\n      error: null,\r\n      next: null,\r\n      currentList: [],\r\n      recommendedList: [],\r\n      alternativeList: [],\r\n      statuses: [\r\n        \"Current\",\r\n        \"Recommended\",\r\n        \"Alternative\"\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    sortPlatforms() {\r\n      this.platforms.forEach((platform) => {\r\n        if (platform.status == \"Current\") {\r\n          this.currentList.push(platform);\r\n        } else if (platform.status == \"Recommended\") {\r\n          this.recommendedList.push(platform);\r\n        } else if (platform.status == \"Alternative\") {\r\n          this.alternativeList.push(platform);\r\n        } else {\r\n          console.log(platform);\r\n        }\r\n      })\r\n    },\r\n    filterByStatus(status) {\r\n      if (status == \"Current\") {\r\n        return this.checkNull(this.currentList);\r\n      } else if (status == \"Recommended\") {\r\n        return this.checkNull(this.recommendedList);\r\n      } else if (status == \"Alternative\") {\r\n        return this.checkNull(this.alternativeList);\r\n      }\r\n    },\r\n    checkNull(list) {\r\n      if (list.length > 0) {\r\n        return list;\r\n      }\r\n    },\r\n    setPageTitle(title) {\r\n      document.title = title;\r\n    },\r\n    getScenarioData() {\r\n      let endpoint = `/api/scenarios/${this.id}/`;\r\n      apiService(endpoint).then(data => {\r\n        if (data) {\r\n          this.scenario = data;\r\n          this.setPageTitle(data.client);\r\n        } else {\r\n          this.scenario = null;\r\n          this.setPageTitle(\"404 - Page Not Found\");\r\n        }\r\n      });\r\n    },\r\n    getScenarioPlatforms() {\r\n      let endpoint = `/api/scenarios/${this.id}/platforms/`;\r\n      if (this.next) {\r\n        endpoint = this.next;\r\n      }\r\n      apiService(endpoint).then(data => {\r\n        this.platforms = data.results;\r\n        this.sortPlatforms();\r\n        if (data.next) {\r\n          this.next = data.next;\r\n        } else {\r\n          this.next = null;\r\n        }\r\n      });\r\n    },\r\n    addPlatform(platform) {\r\n      this.platforms.push(platform);\r\n    },\r\n    deleteObj(objectToDelete) {\r\n      this.$delete(this.platforms, this.platforms.indexOf(objectToDelete));\r\n    },\r\n    updateStatus(platform) {\r\n      console.log(platform);\r\n\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getScenarioData();\r\n    this.getScenarioPlatforms();\r\n    document.title = \"Feez\";\r\n  }\r\n};\r\n</script>\r\n"]}]}