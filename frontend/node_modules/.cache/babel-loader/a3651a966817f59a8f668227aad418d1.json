gn=\"right\" align-self=\"center\">\r\n            <v-autocomplete\r\n              :items=\"scenarios\"\r\n              v-model=\"selectedScenario\"\r\n              no-data-text=\"Client not found\"\r\n              item-text=\"client\"\r\n              return-object\r\n              hide-details\r\n              ref=\"autocomplete\"\r\n              append-icon=\"mdi-magnify\"\r\n              style=\"width: 250px\"\r\n              @change=\"goNewScenario\"/>\r\n          </v-col>\r\n          <v-col align=\"right\" :cols=\"3\">\r\n            <Notifications/>\r\n            <v-btn icon  @click=\"goProfile\">\r\n              <v-icon>mdi-account-outline</v-icon>\r\n            </v-btn>\r\n            <v-btn icon>\r\n              <v-icon>mdi-help-circle-outline</v-icon>\r\n            </v-btn>\r\n            <v-btn icon href=\"/accounts/logout/\">\r\n              <v-icon>mdi-power</v-icon>\r\n            </v-btn>\r\n          </v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </v-toolbar>\r\n    <v-toolbar color=\"teal lighten-4\" short class=\"shrink\">\r\n      <v-container style=\"width: 1012px\">\r\n        <v-row>\r\n\r\n          <v-col align-self=\"center\">\r\n            <v-toolbar-title>\r\n              <v-btn icon @click=\"goHome\">\r\n                <v-icon>mdi-home</v-icon>\r\n              </v-btn>\r\n              <template v-if=\"activeScenario.id\">\r\n                <v-icon>mdi-chevron-right</v-icon>\r\n                <span class=\"scenarioLink\" @click=\"goScenario\">  {{activeScenario.client}}</span>\r\n              </template>\r\n              <template v-if=\"activePlatform.id != null\">\r\n                <v-icon>mdi-chevron-right</v-icon>\r\n                <span>  {{customName}}</span>\r\n              </template>\r\n            </v-toolbar-title>\r\n          </v-col>\r\n          <v-col align=\"right\"  v-if=\"activeScenario.id\">\r\n          <v-btn icon @click=\"goDocs\">\r\n            <v-icon>mdi-file-document-outline</v-icon>\r\n          </v-btn>\r\n          <v-btn icon>\r\n            <v-icon>mdi-share</v-icon>\r\n          </v-btn>\r\n\r\n        </v-col>\r\n      </v-row>\r\n      </v-container>\r\n    </v-toolbar>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport _ from 'lodash'\r\nimport { apiService } from \"@/common/api.service\";\r\nimport Notifications from \"@/components/Notifications.vue\";\r\nimport { mapGetters } from \"vuex\";\r\nimport { SET_ACTIVE_SCENARIO } from \"@/store/mutations.type\";\r\n\r\nexport default {\r\n  components: {\r\n    Notifications,\r\n  },\r\n  data() {\r\n    return {\r\n      scenarios: [],\r\n      selectedScenario: null,\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"activeScenario\", \"activeUser\", \"activePlatform\"]),\r\n    customName() {\r\n      if (this.activePlatform.name == null) {\r\n        return this.activePlatform.custom_name;\r\n      } else if (this.activePlatform.custom_name != null) {\r\n        return this.activePlatform.custom_name;\r\n      } else if (this.activePlatform.name) {\r\n        return this.activePlatform.name.name;\r\n      } else {\r\n        return null;\r\n      }\r\n    },\r\n    // activeScenario() {\r\n    //   return JSON.parse(window.localStorage.getItem('scenario'));\r\n    // },\r\n  },\r\n  methods: {\r\n    getScenarios() {\r\n      let endpoint = \"/api/scenarios/\";\r\n      apiService(endpoint).then(data => {\r\n        this.scenarios = data.results;\r\n      });\r\n    },\r\n    goHome() {\r\n      const path = `/`\r\n      if (this.$route.path !== path)  {\r\n        this.$router.push({ name: 'home' });\r\n        // window.localStorage.removeItem('scenario');\r\n      }\r\n    },\r\n    goNewScenario() {\r\n      if (!_.isEmpty(this.selectedScenario)) {\r\n        this.$refs.autocomplete.blur();\r\n        this.$nextTick(() => {\r\n          this.$store.commit(SET_ACTIVE_SCENARIO, this.selectedScenario);\r\n          this.$router.push({ name: 'scenario', params: { id: this.selectedScenario.id } });\r\n          this.selectedScenario = null;\r\n        });\r\n      }\r\n    },\r\n    goScenario() {\r\n      const pathName = 'scenario'\r\n      if (this.$route.name !== pathName) {\r\n        this.$router.push({ name: 'scenario', params: { id: this.activeScenario.id } });\r\n      }\r\n    },\r\n    goProfile() {\r\n      const path = `/profile`\r\n      if (this.$route.path !== path)  {\r\n        this.$router.push({ name: 'profile' });\r\n      }\r\n    },\r\n    goDocs() {\r\n      if (this.activeScenario.id != null) {\r\n        this.$router.push({ name: 'reports', params: { scenarioId: this.activeScenario.id } });\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getScenarios();\r\n  }\r\n}\r\n</script>\r\n<style media=\"screen\">\r\n  .noTransition::after {\r\n  transition: none !important;\r\n  background-color: transparent !important;\r\n  }\r\n  .scenarioLink {\r\n  cursor: pointer;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/components"}]}