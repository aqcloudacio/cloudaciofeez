AHA,GAlBA;AAuBA,EAAA,IAvBA,kBAuBA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,IADA;AAEA,MAAA,UAAA,EAAA,EAFA;AAGA,MAAA,WAAA,EAAA;AAHA,KAAA;AAKA,GA7BA;AA8BA,EAAA,OAAA,EAAA;AACA,IAAA,gBADA,8BACA;AAAA;;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,QADA,4BACA,KAAA,CAAA,UADA,wBACA,KAAA,CAAA,UADA,2BACA,KAAA,CAAA,aADA;AAAA;AAAA,uBAEA,UAAA,CAAA,QAAA,CAFA;;AAAA;AAEA,gBAAA,IAFA;;AAGA,oCAAA,KAAA,CAAA,UAAA,EAAA,IAAA,4CAAA,IAAA,CAAA,OAAA;;AACA,gBAAA,KAAA,CAAA,iBAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KANA;AAOA,IAAA,iBAPA,+BAOA;AAAA;;AACA,UAAA,KAAA,UAAA,EAAA;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,UAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,cAAA,QAAA,4BAAA,KAAA,UAAA,wBAAA,KAAA,UAAA,2BAAA,KAAA,aAAA,yBAAA,KAAA,UAAA,CAAA,CAAA,EAAA,EAAA,kBAAA;AACA,UAAA,UAAA,CAAA,QAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AAAA;;AACA,kCAAA,MAAA,CAAA,WAAA,EAAA,IAAA,8CAAA,IAAA,CAAA,OAAA;AACA,WAFA;AAGA;AACA;AACA,KAhBA;AAiBA,IAAA,aAjBA,yBAiBA,UAjBA,EAiBA;AACA,WAAA,WAAA,CAAA,IAAA,CAAA,UAAA;AACA,KAnBA;AAoBA,IAAA,SApBA,qBAoBA,cApBA,EAoBA;AACA,WAAA,OAAA,CAAA,KAAA,WAAA,EAAA,KAAA,WAAA,CAAA,OAAA,CAAA,cAAA,CAAA;AACA;AAtBA,GA9BA;AAsDA,EAAA,OAtDA,qBAsDA;AACA,SAAA,gBAAA;AACA;AAxDA,CAAA","sourcesContent":["<template lang=\"html\">\r\n  <div v-if=\"portfolios\">\r\n    <div>\r\n      <form class=\"card\">\r\n        <!-- @submit.prevent=\"onSubmit\"> -->\r\n        <!-- This is just showing the name of the fee set for now as most of the\r\n            time it won't need to be edited. Idea is that on-click it expands and\r\n            becomes editable -->\r\n        <div class=\"card-header px-3\">\r\n          Investments\r\n        </div>\r\n        <!-- <div v-for=\"portfolio in portfolios\"\r\n             :key=\"portfolio.name\">\r\n             <p>{{ portfolio.name }}</p>\r\n        </div> -->\r\n        <hr />\r\n        <div v-for=\"investment in investments\" :key=\"investment.id\">\r\n          <div>\r\n            || {{ investment.name.name }} || ${{ investment.amount }} ||\r\n            {{ feeDescription }} ||\r\n          </div>\r\n          <DeleteObj\r\n            :objectType=\"'investment'\"\r\n            :objectToDelete=\"investment\"\r\n            :portfolioId=\"investment.portfolio\"\r\n            @delete-object=\"deleteObj\"\r\n          />\r\n          <!-- investmentdetail should be in a modal or similar -->\r\n          <InvestmentDetail\r\n            :investment=\"investment\"\r\n            :portfolioId=\"investment.portfolio\"\r\n          />\r\n        </div>\r\n        <AddInvestment @add-investment=\"addInvestment\" v-on=\"$listeners\" />\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service.js\";\r\nimport AddInvestment from \"@/components/AddInvestment.vue\";\r\nimport InvestmentDetail from \"@/components/InvestmentDetail.vue\";\r\nimport DeleteObj from \"@/components/DeleteObj.vue\";\r\n\r\nexport default {\r\n  name: \"InvestmentSummary\",\r\n  inject: [\"platformId\", \"scenarioId\"],\r\n  provide() {\r\n    return {\r\n      platformFeeId: this.platformFeeId\r\n    };\r\n  },\r\n  props: {\r\n    platformFeeId: {\r\n      type: Number,\r\n      required: true\r\n    },\r\n    feeDescription: {\r\n      type: String,\r\n      required: false\r\n    }\r\n  },\r\n  components: {\r\n    AddInvestment,\r\n    DeleteObj,\r\n    InvestmentDetail\r\n  },\r\n  data() {\r\n    return {\r\n      error: null,\r\n      portfolios: [],\r\n      investments: []\r\n    };\r\n  },\r\n  methods: {\r\n    async getPortfolioData() {\r\n      let endpoint = `/api/scenarios/${this.scenarioId}/platforms/${this.platformId}/platformfees/${this.platformFeeId}/portfolios/`;\r\n      let data = await apiService(endpoint);\r\n      this.portfolios.push(...data.results);\r\n      this.getInvestmentData();\r\n    },\r\n    getInvestmentData() {\r\n      if (this.portfolios) {\r\n        for (let i = 0; i < this.portfolios.length; i++) {\r\n          let endpoint = `/api/scenarios/${this.scenarioId}/platforms/${this.platformId}/platformfees/${this.platformFeeId}/portfolios/${this.portfolios[i].id}/investments/`;\r\n          apiService(endpoint).then(data => {\r\n            this.investments.push(...data.results);\r\n          });\r\n        }\r\n      }\r\n    },\r\n    addInvestment(investment) {\r\n      this.investments.push(investment);\r\n    },\r\n    deleteObj(objectToDelete) {\r\n      this.$delete(this.investments, this.investments.indexOf(objectToDelete));\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getPortfolioData();\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}