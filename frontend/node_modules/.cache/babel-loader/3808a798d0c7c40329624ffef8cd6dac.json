AAA,QAAA;AACA,KAvCA;AAwCA,IAAA,iBAxCA,6BAwCA,IAxCA,EAwCA,KAxCA,EAwCA;AACA,UAAA,IAAA,GAAA,KAAA,QAAA,CAAA,KAAA,QAAA,CAAA,IAAA,CAAA,CAAA;AACA,UAAA,QAAA,GAAA,EAAA;;AACA,UAAA,IAAA,IAAA,IAAA,EAAA;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,IAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,cAAA,KAAA,IAAA,IAAA,CAAA,CAAA,CAAA,CAAA,CAAA,CAAA,EAAA;AACA,YAAA,QAAA,GAAA,IAAA,CAAA,CAAA,CAAA,CAAA,CAAA,CAAA;AACA,iBAAA,QAAA,CAAA,IAAA,IAAA,QAAA;AACA;AACA;AACA;AACA,KAnDA;AAoDA,IAAA,QApDA,oBAoDA,IApDA,EAoDA;AACA,aAAA,IAAA,CAAA,MAAA,CAAA,OAAA,CAAA;AACA,KAtDA;AAuDA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,IAAA,cAtEA,4BAsEA;AACA,UAAA,KAAA,QAAA,EAAA;AACA,YAAA,QAAA,oCAAA,KAAA,QAAA,CAAA,EAAA,MAAA;AACA,aAAA,QAAA,CAAA,QAAA,GAAA,IAAA;AACA,eAAA,QAAA;AACA,OAJA,MAIA;AACA,YAAA,SAAA,4BAAA,KAAA,UAAA,wBAAA,KAAA,QAAA,CAAA,EAAA,MAAA;;AACA,aAAA,QAAA,CAAA,MAAA,GAAA,IAAA;AACA,eAAA,SAAA;AACA;AACA,KAhFA;AAiFA,IAAA,SAjFA,uBAiFA;AACA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,YAAA,GAAA,KAAA;AACA,KApFA;AAqFA,IAAA,eArFA,6BAqFA;AAAA;;AACA,UAAA,QAAA,wBAAA;AACA,MAAA,UAAA,CAAA,QAAA,EAAA,KAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,IAAA,CAAA,OAAA;AACA,OAFA;AAGA,KA1FA;AA2FA,IAAA,QA3FA,sBA2FA;AAAA;;AACA,WAAA,cAAA,GAAA,KAAA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,UAAA,QAAA,GAAA,KAAA,cAAA,EAAA;AACA,MAAA,UAAA,CAAA,QAAA,EAAA,OAAA,EAAA,KAAA,QAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,iBAAA,EAAA,IAAA;AACA,OAFA;AAGA;AAnGA,GAtGA;AA2MA,EAAA,OA3MA,qBA2MA;AACA,SAAA,eAAA;AACA;AA7MA,CAAA","sourcesContent":["<template lang=\"html\">\r\n  <v-dialog v-model=\"dialog\">\r\n    <template v-slot:activator=\"{ on }\">\r\n\r\n    <v-card  v-on=\"on\">\r\n      <v-row class=\"mb-0\">\r\n        <v-col class=\"pb-0\">\r\n          <v-card-title>{{ customName }}</v-card-title>\r\n          <v-card-text>\r\n            Balance: {{ platform.platform_total | toCurrency }} <br>\r\n            Fees: {{platform.platform_total_fees | toCurrency }} <br>\r\n            <!-- Unallocated Funds: $XXXXX  don't want to implement this yet-->\r\n          </v-card-text>\r\n        </v-col>\r\n        <v-col class=\"pb-0 pt-6\">\r\n          <v-tooltip right>\r\n            <template v-slot:activator=\"{ on }\">\r\n              <v-btn icon @click.stop v-on=\"on\">\r\n                <v-icon :color=\"isLinked\" medium>mdi-share-variant</v-icon>\r\n              </v-btn>\r\n            </template>\r\n            <template v-for=\"item in platformLinks\">\r\n              <span :key=\"item[0]\">\r\n                {{item[0]}}\r\n                <template v-if=\"item[1]\">\r\n                  : {{item[1] | toCurrency}}<br>\r\n                </template>\r\n              </span>\r\n            </template>\r\n          </v-tooltip>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row class=\"mt-0 mx-2\">\r\n        <v-col>\r\n          <v-btn >\r\n            Platform Detail\r\n          </v-btn>\r\n        </v-col>\r\n        <v-col >\r\n          <v-menu offset-y\r\n          @click.stop\r\n          transition=\"slide-y-transition\"\r\n          :close-on-content-click=\"true\">\r\n            <template v-slot:activator=\"{ on }\">\r\n              <v-btn\r\n                color=\"primary\"\r\n                dark\r\n                v-on=\"on\"\r\n              >\r\n                Fee Detail\r\n              </v-btn>\r\n            </template>\r\n            <v-list>\r\n              <template v-for=\"platformFee in platformFees\">\r\n                  <PlatformFeeSummary\r\n                    v-on=\"$listeners\"\r\n                    :platformFee=\"platformFee\"\r\n                    :key=\"platformFee.id\"\r\n                  />\r\n              </template>\r\n            </v-list>\r\n      </v-menu>\r\n        </v-col>\r\n        <v-col>\r\n          <AddPortfolio v-on=\"$listeners\" />\r\n        </v-col>\r\n\r\n      </v-row>\r\n    </v-card>\r\n  </template>\r\n  <v-container>\r\n    <v-card class=\"pa-4\">\r\n\r\n    <v-form @submit.prevent=\"onSubmit\">\r\n      <v-row>\r\n        <v-col class=\"pt-0\">\r\n          <v-row>\r\n            <v-col>\r\n              <v-card height=\"100%\">\r\n                <v-row>\r\n                  <v-col>\r\n                    <v-card-title v-if=\"editName == false && changePlatform == false\">\r\n                      {{customName}}\r\n                    </v-card-title>\r\n                    <v-text-field\r\n                      class=\"px-4\"\r\n                      v-else-if=\"editName == true\"\r\n                      hide-details\r\n                      :value=\"customName\"\r\n                      @change=\"platform.custom_name = $event\"\r\n                      label=\"Set a custom name\"\r\n                    />\r\n                    <v-combobox\r\n                      class=\"px-4\"\r\n                      v-else-if=\"changePlatform == true\"\r\n                      :value=\"customName\"\r\n                      @input=\"setNewName($event)\"\r\n                      :items=\"platformList\"\r\n                      @keyup.enter=\"closeMenu\"\r\n                      label=\"Swap platforms\"\r\n                      item-text=\"name\"\r\n                      no-data-text=\"Create a custom platform\"\r\n                      ref=\"combobox\"\r\n                      :search-input.sync=\"search\"\r\n                      >\r\n                      <template v-slot:no-data>\r\n                        <v-list-item>\r\n                          <v-list-item-content>\r\n                            <v-list-item-title>\r\n                              No platforms match \"<strong>{{ search }}</strong>\". Press <kbd>enter</kbd> to create a custom platform.\r\n                            </v-list-item-title>\r\n                          </v-list-item-content>\r\n                        </v-list-item>\r\n                      </template>\r\n                    </v-combobox>\r\n                  </v-col>\r\n                  <v-col class=\"mt-4\" :cols=\"2\">\r\n                      <v-icon\r\n\r\n                        color=\"orange\"\r\n                        @click=\"editName = !editName\"\r\n                        large>\r\n                        mdi-pencil-circle-outline\r\n                      </v-icon>\r\n                  </v-col>\r\n                  <v-col :cols=\"2\"  class=\"mt-4\">\r\n\r\n                    <v-dialog v-model=\"confirmDialog\" max-width=\"290\">\r\n                      <template v-slot:activator=\"{ on }\">\r\n                          <v-icon\r\n                            v-on=\"on\"\r\n                            color=\"green\" large>\r\n                            mdi-swap-horizontal-circle-outline\r\n                          </v-icon>\r\n                      </template>\r\n                      <v-container>\r\n                        <v-card>\r\n                          <v-card-text>\r\n                            Warning: swapping platforms will reset all platform fees<br>\r\n                            Do you want to proceed?\r\n                          </v-card-text>\r\n                          <template>\r\n                            <v-btn @click=\"swapPlatform\">\r\n                              Yes\r\n                            </v-btn>\r\n                            <v-btn @click=\"confirmDialog = !confirmDialog\">\r\n                              No\r\n                            </v-btn>\r\n                          </template>\r\n                        </v-card>\r\n                      </v-container>\r\n                    </v-dialog>\r\n                  </v-col>\r\n                </v-row>\r\n                <v-card-text>\r\n                  PDS Date: {{platform.PDS_date}} version {{platform.PDS_version}} <br>\r\n                  Last Checked: {{platform.last_checked}}\r\n                </v-card-text>\r\n                <v-select\r\n                 class=\"pa-4\"\r\n                  :items=\"getReadableList('platform_type')\"\r\n                  @change=\"convertToRealName('platform_type', $event)\"\r\n                  :value=\"convertLongName('platform_type')\"\r\n                  label=\"Account Type\"\r\n                  dense\r\n                  hide-details\r\n                ></v-select>\r\n                <v-text-field\r\n                  class=\"px-4 pb-4\"\r\n                  hide-details\r\n                  v-model=\"platform.account_number\"\r\n                  dense\r\n                  label=\"Account Number\"\r\n                />\r\n              </v-card>\r\n            </v-col>\r\n          </v-row>\r\n          <v-row>\r\n            <v-col :cols=\"4\" >\r\n              <v-btn\r\n                width=\"100%\"\r\n                href=\"platform.PDS_link\">PDS</v-btn>\r\n            </v-col>\r\n            <v-col :cols=\"4\">\r\n              <v-btn\r\n                width=\"100%\"\r\n                href=\"platform.AA_link\">Asset Allocation</v-btn>\r\n            </v-col>\r\n            <v-col :cols=\"4\">\r\n              <v-btn\r\n                width=\"100%\"\r\n                href=\"platform.ICR_link\">Investment Fees</v-btn>\r\n            </v-col>\r\n          </v-row>\r\n          <v-row align=\"end\">\r\n            <v-col>\r\n              <v-btn type=\"submit\" color=\"primary\">\r\n                Save\r\n              </v-btn>\r\n            </v-col>\r\n          </v-row>\r\n        </v-col>\r\n        <v-col>\r\n          <v-card>\r\n            <v-card-title class=\"pb-0\">\r\n              Fee Information\r\n            </v-card-title>\r\n            <v-card-text>\r\n              <v-row class=\"pa-0\">\r\n                <v-col class=\"pt-0\">\r\n                  <v-checkbox\r\n                    hide-details\r\n                    v-model=\"platform.shared_admin_fee\"\r\n                    label=\"Shared admin fee\"\r\n                  />\r\n                  <v-checkbox\r\n                    hide-details\r\n                    v-model=\"platform.single_fee_group\"\r\n                    label=\"Single fee group\"\r\n                  />\r\n                </v-col>\r\n                <v-col  class=\"pt-0\">\r\n                  <v-checkbox\r\n                    hide-details\r\n                    v-model=\"platform.white_label_admin_fee\"\r\n                    label=\"White labeled admin fee\"\r\n                  />\r\n                </v-col>\r\n              </v-row>\r\n              <v-row class=\"mt-0\">\r\n                <v-col  class=\"pt-0\">\r\n                  <v-text-field\r\n                    hide-details\r\n                    v-model=\"platform.cash_fee\"\r\n                    label=\"Cash account fee\"\r\n                  />\r\n                </v-col>\r\n                <v-col  class=\"pt-0\">\r\n                  <v-text-field\r\n                    hide-details\r\n                    v-model=\"platform.switch_fee\"\r\n                    label=\"Switching fee\"\r\n                  />\r\n                </v-col>\r\n              </v-row>\r\n              <v-row class=\"mt-0\">\r\n                <v-col  class=\"pt-0\">\r\n                  <v-text-field\r\n                    hide-details\r\n                    v-model=\"platform.admin_fee_cutoff\"\r\n                    label=\"Admin fee cutoff\"\r\n                  />\r\n                </v-col>\r\n                <v-col  class=\"pt-0\">\r\n                  <v-text-field\r\n                    hide-details\r\n                    v-model=\"platform.ORR_levy_cap\"\r\n                    label=\"ORR Levy cap\"\r\n                  />\r\n                </v-col>\r\n              </v-row>\r\n            </v-card-text>\r\n          </v-card>\r\n          <v-card>\r\n            <v-card-title>\r\n              Fee Linking\r\n            </v-card-title>\r\n            <v-card-text>\r\n              <v-row class=\"ma-0\">\r\n                <v-col class=\"pa-0\" :cols=\"allowedWidth\">\r\n                  <v-select\r\n                    :items=\"getReadableList('allowed_fee_link')\"\r\n                    @change=\"convertToRealName('allowed_fee_link', $event)\"\r\n                    :value=\"convertLongName('allowed_fee_link')\"\r\n                    label=\"Allowable Fee Link\"\r\n                    hide-details\r\n                  ></v-select>\r\n                </v-col>\r\n                <v-col class=\"pa-0\" :cols=\"3\">\r\n                  <v-text-field\r\n                    v-if=\"platform.allowed_fee_link != 'None'\"\r\n                    hide-details\r\n                    v-model=\"platform.maximum_linked_accounts\"\r\n                    label=\"Maximum links\"\r\n                  />\r\n                </v-col>\r\n              </v-row>\r\n              <v-row class=\"ma-0\">\r\n                <v-col class=\"pa-0\">\r\n                  <v-currency-field\r\n                    type=\"number\"\r\n                    v-if=\"platform.allowed_fee_link != 'None'\"\r\n                    hide-details\r\n                    prefix=\"$\"\r\n                    :decimal-length=\"0\"\r\n                    v-model=\"platform.manual_linking_adjustment\"\r\n                    label=\"Manual fee linking adjustment\"\r\n                  />\r\n                </v-col>\r\n              </v-row>\r\n              <v-row class=\"ma-0\"  v-if=\"platform.allowed_fee_link != 'None'\">\r\n                <v-col class=\"pa-0\" :cols=\"typeWidth\">\r\n                  <v-select\r\n                    :items=\"getReadableList('fee_link_type')\"\r\n                    @change=\"convertToRealName('fee_link_type', $event)\"\r\n                    :value=\"convertLongName('fee_link_type')\"\r\n                    label=\"Fee Linking Type\"\r\n                    hide-details\r\n                  ></v-select>\r\n                </v-col>\r\n                <v-col class=\"pa-0\" :cols=\"3\">\r\n                  <v-text-field\r\n                    v-if=\"platform.fee_link_type == 'Reduction'\"\r\n                    hide-details\r\n                    v-model=\"platform.admin_fee_linking_reduction\"\r\n                    label=\"Amount\"\r\n                  />\r\n                </v-col>\r\n              </v-row>\r\n            </v-card-text>\r\n          </v-card>\r\n        </v-col>\r\n    </v-row>\r\n  </v-form>\r\n</v-card>\r\n</v-container>\r\n</v-dialog>\r\n</template>\r\n\r\n<script>\r\nimport AddPortfolio from \"@/components/AddPortfolio.vue\";\r\nimport PlatformFeeSummary from \"@/components/PlatformFeeSummary.vue\";\r\nimport { apiService } from \"@/common/api.service.js\";\r\n\r\nexport default {\r\n  name: \"PlatformDetail\",\r\n  inject: {\r\n    platformId: { default: null },\r\n    scenarioId: { default: null },\r\n  },\r\n  props: {\r\n    platform: {\r\n      type: Object,\r\n      required: true,\r\n    },\r\n    template: {\r\n      type: Boolean,\r\n      required: false,\r\n    },\r\n    platformFees: {\r\n      type: Array,\r\n      required: true,\r\n    },\r\n  },\r\n  components: {\r\n    AddPortfolio,\r\n    PlatformFeeSummary,\r\n  },\r\n  data() {\r\n    return {\r\n      changedName: null,\r\n      valueOn: true,\r\n      confirmDialog: false,\r\n      search: null,\r\n      dialog: false,\r\n      changePlatform: false,\r\n      editName: false,\r\n      error: null,\r\n      displayItemsAdmin: [\r\n          //format is [\"field name\", \"friendly name\", \"editable by\", \"type\", \"tooltip\"]\r\n          [\"status\", \"Platform status\",\"all\",\"list\",null], //hidden for user (user can use Scenario view) /\r\n          [\"platform_type\", \"Platform type\",\"all\",\"list\",null], //editable for user /\r\n          [\"PDS_date\", \"PDS date\",\"admin\",\"date\",null], //read only for user /\r\n          [\"PDS_version\", \"PDS version\",\"admin\",\"string\",null], //read only for user /\r\n          [\"PDS_link\", \"Link to PDS\",\"admin\",\"link\",null], //read-only link /\r\n          [\"AA_link\", \"Link to Asset Allocation\",\"admin\",\"link\",null], //read-only link /\r\n          [\"ICR_link\", \"Link to Investment Fees\",\"admin\",\"link\",null], //read-only link /\r\n          [\"last_checked\", \"Last checked\",\"admin\",\"date\",null], //read only for user /\r\n          [\"notes\",\"Notes\",\"admin\",\"string\",null] //hidden for user /\r\n        ],\r\n      displayItemsMisc: [\r\n          //format is [\"field name\", \"friendly name\", \"editable by\", \"tooltip\"]\r\n          [\"shared_admin_fee\", \"Shared admin fee\",\"all\",\"boolean\",\"If a platform has multiple fee structures, but only one fixed admin fee will be charged\"],\r\n          [\"single_fee_group\", \"Single fee group\",\"all\",\"boolean\",'If there is no \"splitting\" of platform fee groupings. That is, you can only have one fee group for the platform.'],\r\n          [\"allowed_fee_link\", \"Account linking allowed\",\"all\",\"list\",\"The methods allowed to link accounts for the purposes of amin fee splitting.\"],\r\n          [\"maximum_linked_accounts\", \"Number of linked accounts\",\"all\",\"number\",\"Maximum number of accounts that can be linked\"],\r\n          [\"fee_link_type\", \"Fee linking effect\",\"all\",\"list\",\"The way that fee linking modifies the administration fee.\"],\r\n          [\"admin_fee_linking_reduction\", \"Admin fee reduction through linking\",\"all\",\"number\",\"The amount that the admin fee is reduced by linking accounts.\"],\r\n          [\"fee_link_group\", \"Linkable platforms\",\"all\",\"FK\",\"Platforms that can be linked together.\"], //hidden for user /\r\n          [\"admin_fee_cutoff\", \"Fee linking effect\",\"all\",\"number\",\"The way that fee linking modifies the administration fee.\"],\r\n          [\"white_label_admin_fee\", \"White labled admin fee\",\"all\",\"boolean\",\"The platform includes the administration fee in the investment fee of its investment options.\"],\r\n          [\"ORR_levy_cap\", \"Maximum ORR Levy\",\"all\",\"number\",\"The cap on the ORR Levy for the entire platform.\"],\r\n        ],\r\n    };\r\n  },\r\n  computed: {\r\n    platformLinks() {\r\n      if (this.platform.linked_platforms == null) {\r\n        return [[\"No linked platforms\"]]\r\n      } else {\r\n        return this.platform.linked_platforms\r\n      }\r\n    },\r\n    isLinked() {\r\n      if (this.platform.linked_platforms == null) {\r\n        return \"grey\"\r\n      } else {\r\n        return \"blue\"\r\n      }\r\n    },\r\n    allowedWidth: function (){\r\n      if (this.platform.allowed_fee_link != \"None\") {\r\n        return 9;\r\n      } else {\r\n        return 12;\r\n      }\r\n    },\r\n    typeWidth: function (){\r\n      if (this.platform.fee_link_type == \"Reduction\") {\r\n        return 9;\r\n      } else {\r\n        return 12;\r\n      }\r\n    },\r\n    customName: function (){\r\n      if (this.platform.name == null) {\r\n        return this.platform.custom_name;\r\n      } else if (this.platform.custom_name != null) {\r\n        return this.platform.custom_name;\r\n      } else if (this.platform.name) {\r\n        return this.platform.name.name;\r\n      } else {\r\n        return null;\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    swapPlatform() {\r\n      this.changePlatform = !this.changePlatform;\r\n      this.confirmDialog = !this.confirmDialog;\r\n    },\r\n    setNewName(event){\r\n      if (event.id != null) {\r\n        this.platform.name = event;\r\n        this.platform.name_id = event.id;\r\n        this.platform.custom_name = null;\r\n        this.platform.refresh = true;\r\n      } else {\r\n        this.platform.name = null;\r\n        this.platform.name_id = null;\r\n        this.platform.custom_name = event;\r\n        this.platform.refresh = true;\r\n      }\r\n    },\r\n    getReadableList(item){\r\n      let list = this.platform[this.joinList(item)];\r\n      let cleanList = [];\r\n      if (list != null) {\r\n        for (let i=0; i<list.length; i++) {\r\n          cleanList.push(list[i][1]);\r\n        }\r\n      }\r\n      return cleanList\r\n    },\r\n    convertLongName(item){\r\n      let list = this.platform[this.joinList(item)];\r\n      let realName = \"\";\r\n      if (list != null) {\r\n        for (let i=0; i<list.length; i++) {\r\n          if (this.platform[item] == list[i][0]) {\r\n            realName = list[i][1];\r\n          }\r\n        }\r\n      }\r\n      return realName;\r\n    },\r\n    convertToRealName(item, event){\r\n      let list = this.platform[this.joinList(item)];\r\n      let realName = \"\";\r\n      if (list != null) {\r\n        for (let i=0; i<list.length; i++) {\r\n          if (event == list[i][1]) {\r\n            realName = list[i][0];\r\n            this.platform[item] = realName;\r\n          }\r\n        }\r\n      }\r\n    },\r\n    joinList(item){\r\n      return item.concat(\"_list\");\r\n    },\r\n    // filteredItems(feeItem){\r\n    //   if (this.expandFees) {\r\n    //     return this.displayItems;\r\n    //   } else {\r\n    //     return this.displayItems.filter(i => feeItem[i[1]] > 0);\r\n    //   };\r\n    // },\r\n    // setPercentage(event, item, fee) {\r\n    //   const i = this.platformFees.findIndex(item => item.id === fee.id);\r\n    //   this.platformFees[i][item[1]] = event / 100 * fee.platform_fee_total;\r\n    //   this.platformFees[i][item[2]] = event / 100;\r\n    // },\r\n    // getPercentage(item, fee) {\r\n    //   return (fee[item[2]]*100)\r\n    // },\r\n    templateToggle() {\r\n      if (this.template){\r\n        let endpoint = `/api/platformtemplates/${this.platform.id}/`;\r\n        this.platform.template = true;\r\n        return endpoint\r\n      } else {\r\n        let endpoint = `/api/scenarios/${this.scenarioId}/platforms/${this.platform.id}/`;\r\n        this.platform.edited = true;\r\n        return endpoint\r\n      }\r\n    },\r\n    closeMenu() {\r\n      // closes the comnbobox dropdown - vuetify bug\r\n      this.$refs.combobox.isMenuActive = false;\r\n    },\r\n    getPlatformList() {\r\n      let endpoint = `/api/platformnames/`;\r\n      apiService(endpoint, \"GET\").then(data => {\r\n        this.platformList = data.results;\r\n      });\r\n    },\r\n    onSubmit() {\r\n      this.changePlatform = false;\r\n      this.editName = false;\r\n      this.dialog = false;\r\n      let endpoint = this.templateToggle();\r\n      apiService(endpoint, \"PATCH\", this.platform).then(data => {\r\n        this.$emit(\"modify-platform\", data);\r\n      });\r\n    }\r\n  },\r\n  created() {\r\n    this.getPlatformList();\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}