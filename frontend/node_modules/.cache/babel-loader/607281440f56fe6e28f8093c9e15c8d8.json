A;AAqCA,IAAA,OArCA,qBAqCA;AACA,UAAA,KAAA,WAAA,CAAA,WAAA,IAAA,IAAA,EAAA;AACA,YAAA,MAAA,GAAA,KAAA,WAAA,CAAA,WAAA,CAAA,GAAA,CAAA,UAAA,CAAA;AAAA,iBAAA,MAAA,CAAA,CAAA,CAAA,UAAA,CAAA;AAAA,SAAA,CAAA;AACA,eAAA,MAAA;AACA,OAHA,MAGA;AACA,eAAA,EAAA;AACA;AACA,KA5CA;AA6CA,IAAA,MA7CA,oBA6CA;AACA,UAAA,MAAA,GAAA,EAAA;AACA,UAAA,KAAA,GAAA,KAAA;;AACA,WAAA,IAAA,GAAA,IAAA,KAAA,OAAA,EAAA;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,SAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,cAAA,GAAA,IAAA,KAAA,SAAA,CAAA,CAAA,EAAA,EAAA,EAAA;AACA,YAAA,MAAA,CAAA,IAAA,CAAA,MAAA,CAAA,KAAA,SAAA,CAAA,CAAA,EAAA,QAAA,GAAA,KAAA,QAAA,CAAA,cAAA,CAAA;AACA,YAAA,KAAA,GAAA,IAAA;AACA;AACA;AACA;;AACA,YAAA,CAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AACA;AACA;;AACA,aAAA,MAAA;AACA,KA7DA;AA8DA,IAAA,SA9DA,uBA8DA;AACA,aAAA;AACA,QAAA,MAAA,EAAA,MAAA,CAAA,MAAA,CAAA,KAAA,OAAA,CADA;AAEA,QAAA,QAAA,EAAA,CACA;AACA,UAAA,IAAA,EAAA,KAAA,OADA;AAEA,UAAA,eAAA,EAAA,qBAFA;AAGA,UAAA,WAAA,EAAA,mBAHA;AAIA,UAAA,WAAA,EAAA,CAJA;AAKA,UAAA,KAAA,EAAA;AALA,SADA,EAQA;AACA,UAAA,IAAA,EAAA,KAAA,MADA;AAEA,UAAA,eAAA,EAAA,yBAFA;AAGA,UAAA,WAAA,EAAA,uBAHA;AAIA,UAAA,WAAA,EAAA,CAJA;AAKA,UAAA,KAAA,EAAA,KAAA;AALA,SARA;AAFA,OAAA;AAmBA;AAlFA,GArEA;AAyJA,EAAA,KAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,WAAA,YAAA;AACA;AAHA,GAzJA;AA8JA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,2BACA;AACA,WAAA,KAAA,IAAA,CAAA;AACA,KAHA;AAIA,IAAA,YAJA,0BAIA;AAAA;;AACA,UAAA,KAAA,QAAA,IAAA,IAAA,EAAA;AACA,YAAA,KAAA,QAAA,CAAA,EAAA,IAAA,IAAA,EAAA;AACA,cAAA,QAAA,4BAAA,KAAA,UAAA,wBAAA,KAAA,QAAA,CAAA,EAAA,gBAAA;AACA,UAAA,UAAA,CAAA,QAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,KAAA,CAAA,SAAA,GAAA,IAAA,CAAA,OAAA;AACA,WAFA;AAGA;AACA;AACA,KAbA;AAcA,IAAA,eAdA,6BAcA;AAAA;;AACA,UAAA,QAAA,iCAAA;AACA,MAAA,UAAA,CAAA,QAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,IAAA,CAAA,OAAA;AACA,OAFA;AAGA,KAnBA;AAoBA,IAAA,eApBA,6BAoBA;AAAA;;AACA,UAAA,QAAA,4BAAA,KAAA,UAAA,MAAA;AACA,MAAA,UAAA,CAAA,QAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,QAAA,MAAA,CAAA,eAAA;AACA,OAHA;AAIA;AA1BA,GA9JA;AA0LA,EAAA,OA1LA,qBA0LA;AACA,SAAA,eAAA;AACA,SAAA,YAAA;AACA;AA7LA,CAAA","sourcesContent":["<template lang=\"html\">\r\n  <v-card v-if=\"chartData != null\" >\r\n      <AABarChart\r\n        :chartData=\"chartData\"\r\n        :options=\"chartOptions\"\r\n        :height=\"150\"\r\n        ref=\"AABarChart\"\r\n        :key=\"AAKey\"\r\n      >\r\n    </AABarChart>\r\n\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nimport AABarChart from \"@/components/AABarChart.vue\";\r\nimport { apiService } from \"@/common/api.service.js\";\r\n\r\nexport default {\r\n  name: \"PlatformAAChartSummary\",\r\n  inject: [\"scenarioId\"],\r\n  props: {\r\n    platform: {\r\n      type: Object,\r\n      required: true,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      AAKey:0,\r\n      RPAANames: [],\r\n      AASummary: [],\r\n      scenario: {},\r\n      chartOptions: {\r\n        scales: {\r\n            yAxes: [{\r\n                ticks: {\r\n                    beginAtZero: true,\r\n                    maxTicksLimit: 6,\r\n                    callback: function (value) {\r\n                      return value.toLocaleString('en-US', {style:'percent'});\r\n                    }\r\n                }\r\n            }],\r\n            xAxes: [{\r\n                ticks: {\r\n                  maxRotation: 0 // angle in degrees\r\n                }\r\n            }],\r\n        },\r\n        title: {\r\n          display: true,\r\n          text: \"Asset Allocation\",\r\n        },\r\n        legend: {\r\n          position: 'top',\r\n          labels: {\r\n            fontColor: 'grey'\r\n          },\r\n          onHover: function(e) {\r\n            e.target.style.cursor = 'pointer';\r\n          },\r\n          onClick: function(e, legendItem) {\r\n            var index = legendItem.datasetIndex;\r\n            var ci = this.chart;\r\n            var meta = ci.getDatasetMeta(index);\r\n\r\n              if (meta.hidden == true) {\r\n                meta.hidden = false\r\n              } else {\r\n                meta.hidden = true;\r\n              }\r\n            ci.update();\r\n          }\r\n        },\r\n        hover: {\r\n         onHover: function(e) {\r\n            var point = this.getElementAtEvent(e);\r\n            if (!point.length) e.target.style.cursor = 'default';\r\n         }\r\n       }\r\n      }\r\n    };\r\n  },\r\n  components: {\r\n    AABarChart\r\n  },\r\n  computed: {\r\n    customName: function (){\r\n      if (this.platform.name == null) {\r\n        return this.platform.custom_name;\r\n      } else if (this.platform.name) {\r\n        return this.platform.name.name;\r\n      } else {\r\n        return \"Loading...\";\r\n      }\r\n    },\r\n    riskProfile() {\r\n      if (this.scenario.risk_profile != undefined) {\r\n        return this.scenario.risk_profile;\r\n      } else {\r\n        return {}\r\n      }\r\n    },\r\n    nameSet: function (){\r\n      let AANameList = {};\r\n      let checkList = []\r\n      for (let i = 0; i < this.RPAANames.length; i++) {\r\n        let found = false;\r\n        let aaName = this.RPAANames[i];\r\n          if (checkList.includes(aaName.custom_name)) {\r\n            found = true;\r\n          }\r\n          if (!found) {\r\n            // THE SPACE ADDED BELOW ALLOWS THE OBJECT TO RETAIN\r\n            // ORDERING OF ITS INTEGER PROPERTIES. THE VUE CONSOLE REMOVES THIS\r\n            // SPACE BUT BROWSER READS IT CORRECTLY.\r\n            AANameList[' '+aaName.id] = aaName.custom_name;\r\n            found = false;\r\n            checkList.push(aaName.custom_name);\r\n          }\r\n      }\r\n      return (AANameList)\r\n    },\r\n    RPArray() {\r\n      if (this.riskProfile.allocations != null) {\r\n        let result = this.riskProfile.allocations.map(a => Number(a.percentage) );\r\n        return result;\r\n      } else {\r\n        return []\r\n      }\r\n    },\r\n    AArray() {\r\n      let result = []\r\n      let found = false;\r\n      for (let key in this.nameSet) {\r\n        for (let i=0; i < this.AASummary.length; i++) {\r\n          if (key == this.AASummary[i].id) {\r\n            result.push(Number(this.AASummary[i].aa_total / this.platform.platform_total))\r\n            found = true\r\n            break\r\n          }\r\n        }\r\n        if (!found) {\r\n          result.push(0);\r\n        }\r\n      }\r\n      return result;\r\n    },\r\n    chartData(){\r\n      return {\r\n        labels: Object.values(this.nameSet),\r\n        datasets: [\r\n          {\r\n            data: this.RPArray,\r\n            backgroundColor: 'rgba(0,128,128,0.2)',\r\n            borderColor: 'rgba(0,128,128,1)',\r\n            borderWidth: 1,\r\n            label: \"Risk Profile\"\r\n          },\r\n          {\r\n            data: this.AArray,\r\n            backgroundColor: 'rgba(255, 99, 132, 0.2)',\r\n            borderColor: 'rgba(255, 99, 132, 1)',\r\n            borderWidth: 1,\r\n            label: this.customName\r\n          }\r\n        ]\r\n      }\r\n    },\r\n  },\r\n  watch: {\r\n    platform () {\r\n      this.getAASummary();\r\n    },\r\n  },\r\n  methods: {\r\n    forceRerender(){\r\n      this.AAKey += 1;\r\n    },\r\n    getAASummary() {\r\n      if (this.platform != null) {\r\n        if (this.platform.id != null) {\r\n          let endpoint = `/api/scenarios/${this.scenarioId}/platforms/${this.platform.id}/aasummary/`;\r\n          apiService(endpoint).then(data => {\r\n            this.AASummary = data.results;\r\n          });\r\n        }\r\n      }\r\n    },\r\n    getRPAANameData() {\r\n      let endpoint = `/api/activeriskprofilenames/`;\r\n      apiService(endpoint).then(data => {\r\n        this.RPAANames = data.results;\r\n      });\r\n    },\r\n    getScenarioData() {\r\n      let endpoint = `/api/scenarios/${this.scenarioId}/`;\r\n      apiService(endpoint).then(data => {\r\n          this.scenario = data;\r\n          this.getRPAANameData();\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getScenarioData();\r\n    this.getAASummary();\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}