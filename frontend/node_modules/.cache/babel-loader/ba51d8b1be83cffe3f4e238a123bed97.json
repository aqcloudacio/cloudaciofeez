AEA,IAAA,KAAA,cAAA,CAAA,WAAA,IAAA,IAAA,EAAA;AACA,eAAA,KAAA,cAAA,CAAA,WAAA;AACA,OAFA,MAEA,IAAA,KAAA,cAAA,CAAA,IAAA,EAAA;AACA,eAAA,KAAA,cAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAFA,MAEA;AACA,eAAA,IAAA;AACA;AACA,KAXA;AAYA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,IAAA,cApBA,4BAoBA;AACA,aAAA,IAAA,CAAA,KAAA,CAAA,MAAA,CAAA,YAAA,CAAA,OAAA,CAAA,UAAA,CAAA,CAAA;AACA;AAtBA,GAxBA;AAgDA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,0BACA;AAAA;;AACA,UAAA,QAAA,GAAA,iBAAA;AACA,MAAA,UAAA,CAAA,QAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA,CAAA,OAAA;AACA,OAFA;AAGA,KANA;AAOA,IAAA,MAPA,oBAOA;AACA,UAAA,IAAA,MAAA;;AACA,UAAA,KAAA,MAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA,QAAA,MAAA,CAAA,YAAA,CAAA,UAAA,CAAA,UAAA;AACA;AACA,KAbA;AAcA,IAAA,UAdA,wBAcA;AAAA;;AACA,UAAA,KAAA,cAAA,CAAA,EAAA,IAAA,IAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,UAAA;AAAA,UAAA,MAAA,EAAA;AAAA,YAAA,EAAA,EAAA,KAAA,cAAA,CAAA;AAAA;AAAA,SAAA;AACA,OAFA,MAEA,IAAA,KAAA,gBAAA,IAAA,IAAA,EAAA;AACA,aAAA,KAAA,CAAA,YAAA,CAAA,IAAA;AACA,aAAA,SAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA,UAAA;AAAA,YAAA,MAAA,EAAA;AAAA,cAAA,EAAA,EAAA,MAAA,CAAA;AAAA;AAAA,WAAA;;AACA,UAAA,MAAA,CAAA,gBAAA,GAAA,IAAA;AACA,SAHA;AAIA;AACA,KAxBA;AAyBA,IAAA,SAzBA,uBAyBA;AACA,UAAA,IAAA,aAAA;;AACA,UAAA,KAAA,MAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA;AACA,KA9BA;AA+BA,IAAA,MA/BA,oBA+BA;AACA,UAAA,KAAA,cAAA,CAAA,EAAA,IAAA,IAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,SAAA;AAAA,UAAA,MAAA,EAAA;AAAA,YAAA,UAAA,EAAA,KAAA,cAAA,CAAA,EAAA;AACA,YAAA,QAAA,EAAA,KAAA,cADA;AAEA,YAAA,IAAA,EAAA,KAAA;AAFA;AAAA,SAAA;AAGA;AACA;AArCA,GAhDA;AAuFA,EAAA,OAvFA,qBAuFA;AACA,SAAA,YAAA;AACA;AAzFA,CAAA","sourcesContent":["<template lang=\"html\">\r\n  <div>\r\n    <v-toolbar color=\"teal lighten-2\" dark dense prominent  class=\"shrink\">\r\n      <v-container style=\"width: 1012px\">\r\n        <v-row>\r\n          <v-col align-self=\"center\" :cols=\"6\">\r\n            <v-toolbar-title large>Stencil</v-toolbar-title>\r\n          </v-col>\r\n          <v-col :cols=\"3\" align=\"right\" align-self=\"center\">\r\n            <v-autocomplete\r\n              :items=\"scenarios\"\r\n              v-model=\"selectedScenario\"\r\n              no-data-text=\"Client not found\"\r\n              item-text=\"client\"\r\n              item-value=\"id\"\r\n              hide-details\r\n              ref=\"autocomplete\"\r\n              append-icon=\"mdi-magnify\"\r\n              style=\"width: 250px\"\r\n              @change=\"goScenario\"/>\r\n          </v-col>\r\n          <v-col align=\"right\" :cols=\"3\">\r\n            <Notifications\r\n             :user=\"user\"/>\r\n            <v-btn icon  @click=\"goProfile\">\r\n              <v-icon>mdi-account-outline</v-icon>\r\n            </v-btn>\r\n            <v-btn icon>\r\n              <v-icon>mdi-help-circle-outline</v-icon>\r\n            </v-btn>\r\n            <v-btn icon href=\"/accounts/logout/\">\r\n              <v-icon>mdi-power</v-icon>\r\n            </v-btn>\r\n          </v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </v-toolbar>\r\n    <v-toolbar color=\"teal lighten-4\" short class=\"shrink\">\r\n      <v-container style=\"width: 1012px\">\r\n        <v-row>\r\n\r\n          <v-col align-self=\"center\">\r\n            <v-toolbar-title>\r\n              <v-btn icon @click=\"goHome\">\r\n                <v-icon>mdi-home</v-icon>\r\n              </v-btn>\r\n              <template v-if=\"activeScenario != null\">\r\n                <v-icon>mdi-chevron-right</v-icon>\r\n                <span class=\"scenarioLink\" @click=\"goScenario\">  {{activeScenario.client}}</span>\r\n              </template>\r\n              <template v-if=\"activePlatform.id != null\">\r\n                <v-icon>mdi-chevron-right</v-icon>\r\n                <span>  {{customName}}</span>\r\n              </template>\r\n            </v-toolbar-title>\r\n          </v-col>\r\n          <v-col align=\"right\"  v-if=\"activeScenario != null\">\r\n          <v-btn icon @click=\"goDocs\">\r\n            <v-icon>mdi-file-document-outline</v-icon>\r\n          </v-btn>\r\n          <v-btn icon>\r\n            <v-icon>mdi-share</v-icon>\r\n          </v-btn>\r\n\r\n        </v-col>\r\n      </v-row>\r\n      </v-container>\r\n    </v-toolbar>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service\";\r\nimport Notifications from \"@/components/Notifications.vue\";\r\n\r\nexport default {\r\n  props: {\r\n    // activeScenario: {\r\n    //   type: Object,\r\n    //   required: false,\r\n    // },\r\n    activePlatform: {\r\n      type: Object,\r\n      required: false,\r\n    },\r\n    user: {\r\n      type: Object,\r\n      required: true,\r\n    },\r\n  },\r\n  components: {\r\n    Notifications,\r\n  },\r\n  data() {\r\n    return {\r\n      scenarios: [],\r\n      selectedScenario: null,\r\n    }\r\n  },\r\n  computed: {\r\n    customName() {\r\n      if (this.activePlatform.name == null) {\r\n        return this.activePlatform.custom_name;\r\n      } else if (this.activePlatform.custom_name != null) {\r\n        return this.activePlatform.custom_name;\r\n      } else if (this.activePlatform.name) {\r\n        return this.activePlatform.name.name;\r\n      } else {\r\n        return null;\r\n      }\r\n    },\r\n    // activeScenarioComputed: {\r\n    //   get() {\r\n    //     return this.activeScenario\r\n    //   },\r\n    //   set(newValue) {\r\n    //     this.$emit(\"scenario-activated\", newValue)\r\n    //   },\r\n    // },\r\n    activeScenario() {\r\n      return JSON.parse(window.localStorage.getItem('scenario'));\r\n    },\r\n  },\r\n  methods: {\r\n    getScenarios() {\r\n      let endpoint = \"/api/scenarios/\";\r\n      apiService(endpoint).then(data => {\r\n        this.scenarios = data.results;\r\n      });\r\n    },\r\n    goHome() {\r\n      const path = `/`\r\n      if (this.$route.path !== path)  {\r\n        this.$router.push({ name: 'home' });\r\n        window.localStorage.removeItem('scenario');\r\n      }\r\n    },\r\n    goScenario() {\r\n      if (this.activePlatform.id != null) {\r\n        this.$router.push({ name: 'scenario', params: { id: this.activeScenario.id } });\r\n      } else if (this.selectedScenario != null) {\r\n        this.$refs.autocomplete.blur();\r\n        this.$nextTick(() => {\r\n          this.$router.push({ name: 'scenario', params: { id: this.selectedScenario } });\r\n          this.selectedScenario = null;\r\n        })\r\n      }\r\n    },\r\n    goProfile() {\r\n      const path = `/profile`\r\n      if (this.$route.path !== path)  {\r\n        this.$router.push({ name: 'profile' });\r\n      }\r\n    },\r\n    goDocs() {\r\n      if (this.activeScenario.id != null) {\r\n        this.$router.push({ name: 'reports', params: { scenarioId: this.activeScenario.id,\r\n                                                      scenario:this.activeScenario,\r\n                                                      user:this.user } });\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getScenarios();\r\n  }\r\n}\r\n</script>\r\n<style media=\"screen\">\r\n  .noTransition::after {\r\n  transition: none !important;\r\n  background-color: transparent !important;\r\n  }\r\n  .scenarioLink {\r\n  cursor: pointer;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/components"}]}