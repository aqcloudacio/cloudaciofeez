tail.name}} <br>\r\n          {{userDetail.email}} <br>\r\n          {{userDetail.AFSL}} <br>\r\n        </v-card>\r\n      </v-col>\r\n      <v-col :cols=\"5\">\r\n        <v-card>\r\n          Stats?  <br>\r\n          preferences? <br>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n    <v-row >\r\n      <v-col :cols=\"3\">\r\n        <v-card\r\n          height=\"100%\"\r\n          min-height=\"100px\"\r\n          class=\"text-center\"\r\n          @click=\"active='themes'\">\r\n          <v-card-title class=\"justify-center\">\r\n            Theme\r\n          </v-card-title>\r\n          <v-chip\r\n              v-if=\"userDetail.active_theme_name\"\r\n               label\r\n              color=\"teal lighten-4\"\r\n              >\r\n              {{userDetail.active_theme_name}}\r\n            </v-chip>\r\n        </v-card>\r\n      </v-col>\r\n      <v-col :cols=\"3\">\r\n        <v-card\r\n          height=\"100%\"\r\n          min-height=\"100px\"\r\n          :class=\"userDetail.active_practice_name ? 'text-center' : 'd-flex justify-center'\"\r\n          @click=\"active='practices'\">\r\n\r\n          <v-card-title class=\"justify-center\">\r\n            Practice\r\n          </v-card-title>\r\n          <v-chip\r\n              v-if=\"userDetail.active_practice_name\"\r\n               label\r\n              color=\"teal lighten-4\"\r\n              >\r\n              {{userDetail.active_practice_name}}\r\n            </v-chip>\r\n\r\n        </v-card>\r\n      </v-col>\r\n      <v-col :cols=\"3\">\r\n        <v-card\r\n          height=\"100%\"\r\n          min-height=\"100px\"\r\n          :class=\"activeRPName ? 'text-center' : 'd-flex justify-center'\"\r\n          @click=\"active='riskProfile'\">\r\n          <v-card-title class=\"justify-center\">\r\n            Risk Profile\r\n          </v-card-title>\r\n          <v-chip\r\n            v-if=\"activeRPName\"\r\n            class=\"mb-4\" label\r\n            color=\"teal lighten-4\">\r\n            {{activeRPName}}\r\n          </v-chip>\r\n        </v-card>\r\n      </v-col>\r\n      <v-col :cols=\"3\">\r\n        <v-card\r\n          height=\"100%\"\r\n          min-height=\"100px\"\r\n          class=\" d-flex justify-center\"\r\n          @click=\"active='portfolio'\">\r\n          <v-card-title>\r\n            Model Portfolios\r\n          </v-card-title>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n  <v-card>\r\n    <v-container>\r\n      <ThemeTable\r\n        v-if=\"active == 'themes'\"\r\n        :user=\"userDetail\"\r\n        @update-user=\"userDetail = $event\"\r\n        @update-active-theme=\"updateActiveTheme\"/>\r\n      <PortfolioTable\r\n        v-if=\"active == 'portfolio'\"\r\n        :user=\"userDetail\"/>\r\n      <PracticeTable\r\n        v-if=\"active == 'practices'\"\r\n        :riskProfileGroups=\"riskProfileGroups\"\r\n        @update-user=\"userDetail = $event\"\r\n        @update-active-practice=\"updateActivePractice\"\r\n        :user = \"userDetail\" />\r\n      <RiskProfileTable\r\n        v-if=\"active == 'riskProfile'\"\r\n        :riskProfileGroups=\"riskProfileGroups\"\r\n        :user = \"userDetail\"\r\n        @add-rpgroup=\"addRPGroup($event)\"\r\n        @update-rpgroup=\"updateRPGroup($event)\"\r\n        @update-user=\"userDetail = $event\"\r\n        @delete-rpgroup=\"removeRPGroup($event)\"\r\n       />\r\n    </v-container>\r\n  </v-card >\r\n</v-container>\r\n\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service\";\r\nimport RiskProfileTable from \"@/components/RiskProfileTable.vue\";\r\nimport ThemeTable from \"@/components/ThemeTable.vue\";\r\nimport PortfolioTable from \"@/components/PortfolioTable.vue\";\r\nimport PracticeTable from \"@/components/PracticeTable.vue\";\r\n\r\n\r\nexport default {\r\n  name: \"profile\",\r\n  data() {\r\n    return {\r\n      riskProfileGroups: null,\r\n      active: 'themes',\r\n      userDetail: {},\r\n    };\r\n  },\r\n  components: {\r\n    RiskProfileTable,\r\n    PortfolioTable,\r\n    PracticeTable,\r\n    ThemeTable,\r\n  },\r\n  computed: {\r\n    practices() {\r\n      if (this.userDetail.practice_names) {\r\n        return this.userDetail.practice_names\r\n      } else {\r\n        return null\r\n      }\r\n    },\r\n    activeRPName() {\r\n      if (this.riskProfileGroups && this.userDetail) {\r\n        const result = this.riskProfileGroups.find(\r\n            x => x.id === this.userDetail.active_rp)\r\n            if (result) {\r\n              //for some reason splicing the array causes this to temporarily\r\n              // not find the riskProfileGroup array. This If statement avoids\r\n              // a warning message.\r\n              return result.name\r\n            } else {\r\n              return null\r\n            }\r\n      } else {\r\n        return null\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    updateActivePractice(data){\r\n      this.userDetail.active_practice = data.id;\r\n      this.userDetail.active_practice_name = data.name;\r\n    },\r\n    updateActiveTheme(data){\r\n      this.userDetail.active_theme = data.id;\r\n      this.userDetail.active_theme_name = data.name;\r\n    },\r\n    addRPGroup() {\r\n      this.getRPGroups();\r\n      // this.riskProfileGroups.push(data);\r\n    },\r\n    removeRPGroup(data) {\r\n      const i = this.riskProfileGroups.findIndex(item => item.id === data.id);\r\n      this.riskProfileGroups.splice(i);\r\n    },\r\n    updateRPGroup(data) {\r\n      const i = this.riskProfileGroups.findIndex(item => item.id === data.id);\r\n      this.riskProfileGroups.splice(i,0,data);\r\n    },\r\n    getRPGroups() {\r\n      let endpoint = `/api/riskprofilegroups/`;\r\n      apiService(endpoint).then(data => {\r\n        this.riskProfileGroups = data.results;\r\n      });\r\n    },\r\n    getUserData() {\r\n      let endpoint = `/api/user/`;\r\n      apiService(endpoint).then(data => {\r\n        this.userDetail = data.results[0];\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getRPGroups();\r\n    this.getUserData();\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views"}]}