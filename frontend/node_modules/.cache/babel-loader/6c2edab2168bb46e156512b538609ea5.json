e: 'output', params: { scenarioId:this.id } }\"\r\n              color=\"secondary\"\r\n              justify=\"right\">\r\n                  Results\r\n        </v-btn>\r\n      <v-row>\r\n        <v-col>\r\n          <v-row>\r\n            <v-col>\r\n              HEALTHCHECK\r\n            </v-col>\r\n          </v-row>\r\n          <v-row>\r\n              <v-col>\r\n                Fees\r\n              </v-col>\r\n          </v-row>\r\n        </v-col>\r\n        <v-col>\r\n          AA CHART\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n    <v-container\r\n      class=\"grey lighten-2\">\r\n      <v-row>\r\n        <v-col\r\n          v-for=\"status in statuses\"\r\n          :key=\"status.index\">\r\n          <v-card>\r\n            <v-card-title\r\n              class=\"justify-center\">\r\n            {{ status }}\r\n            </v-card-title>\r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row>\r\n        <v-col\r\n          v-for=\"status in statuses\"\r\n          :key=\"status.index\">\r\n          <AddPlatform\r\n            :status=\"status\"\r\n            @add-platform=\"addPlatform\" />\r\n        </v-col>\r\n      </v-row>\r\n      <v-row id=\"platformList\">\r\n        <v-col\r\n          v-for=\"status in statuses\"\r\n          :key=\"status.index\">\r\n          <v-row>\r\n            <v-col\r\n              :cols=\"12\"\r\n              v-for=\"platform in filterByStatus(status)\"\r\n              :key=\"platform.id\">\r\n              <PlatformSummary\r\n                :scenario=\"scenario\"\r\n                :platform=\"platform\"\r\n                @delete-object=\"deleteObj\"\r\n              />\r\n            </v-col>\r\n          </v-row>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n\r\n    <AddCustomPlatform />\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service.js\";\r\nimport AddPlatform from \"@/components/AddPlatform.vue\";\r\nimport AddCustomPlatform from \"@/components/AddCustomPlatform.vue\";\r\nimport PlatformSummary from \"@/components/PlatformSummary.vue\";\r\n\r\nexport default {\r\n  name: \"Scenario\",\r\n  provide() {\r\n    return {\r\n      scenarioId: this.id\r\n    };\r\n  },\r\n  props: {\r\n    id: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  components: {\r\n    AddPlatform,\r\n    PlatformSummary,\r\n    AddCustomPlatform,\r\n  },\r\n  data() {\r\n    return {\r\n      scenario: {},\r\n      platforms: [],\r\n      error: null,\r\n      next: null,\r\n      statuses: [\r\n        \"Current\",\r\n        \"Recommended\",\r\n        \"Alternative\"\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    filterByStatus(status) {\r\n        return this.platforms.filter(function (platform) {\r\n          return platform.status == status\r\n        })\r\n    },\r\n    setPageTitle(title) {\r\n      document.title = title;\r\n    },\r\n    getScenarioData() {\r\n      let endpoint = `/api/scenarios/${this.id}/`;\r\n      apiService(endpoint).then(data => {\r\n        if (data) {\r\n          this.scenario = data;\r\n          this.setPageTitle(data.client);\r\n        } else {\r\n          this.scenario = null;\r\n          this.setPageTitle(\"404 - Page Not Found\");\r\n        }\r\n      });\r\n    },\r\n    getScenarioPlatforms() {\r\n      let endpoint = `/api/scenarios/${this.id}/platforms/`;\r\n      if (this.next) {\r\n        endpoint = this.next;\r\n      }\r\n      apiService(endpoint).then(data => {\r\n        this.platforms = data.results;\r\n        if (data.next) {\r\n          this.next = data.next;\r\n        } else {\r\n          this.next = null;\r\n        }\r\n      });\r\n    },\r\n    addPlatform(platform) {\r\n      this.platforms.push(platform);\r\n    },\r\n    deleteObj(objectToDelete) {\r\n      this.$delete(this.platforms, this.platforms.indexOf(objectToDelete));\r\n    },\r\n    createSortable() {\r\n      let table = document.getElementById(\"platformList\");\r\n      console.log(table);\r\n      const _self = this;\r\n      Sortable.create(table, {\r\n        handle: \".handle\",\r\n         onEnd({ newIndex, oldIndex }) {\r\n         const rowSelected = _self.desserts.splice(oldIndex, 1)[0];\r\n         _self.desserts.splice(newIndex, 0, rowSelected);\r\n  }\r\n});\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getScenarioData();\r\n    this.getScenarioPlatforms();\r\n    this.createSortable();\r\n    document.title = \"Feez\";\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views"}]}