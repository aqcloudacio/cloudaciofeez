,KAAA,QAAA,CAAA,IAAA,CAAA,CAAA,CAAA,KAAA,IAAA,CAAA,CAAA,CAAA,CAAA,CAAA,CAAA,EAAA;AACA,UAAA,QAAA,GAAA,IAAA,CAAA,CAAA,CAAA,CAAA,CAAA,CAAA;AACA;AACA;;AACA,aAAA,QAAA;AACA,KAlBA;AAmBA,IAAA,iBAnBA,6BAmBA,IAnBA,EAmBA,KAnBA,EAmBA;AACA,UAAA,IAAA,GAAA,KAAA,QAAA,CAAA,KAAA,QAAA,CAAA,IAAA,CAAA,CAAA,CAAA,CAAA,CAAA;AACA,UAAA,QAAA,GAAA,EAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,IAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,KAAA,IAAA,IAAA,CAAA,CAAA,CAAA,CAAA,CAAA,CAAA,EAAA;AACA,UAAA,QAAA,GAAA,IAAA,CAAA,CAAA,CAAA,CAAA,CAAA,CAAA;AACA,eAAA,QAAA,CAAA,IAAA,CAAA,CAAA,CAAA,IAAA,QAAA;AACA;AACA;AACA,KA5BA;AA6BA,IAAA,QA7BA,oBA6BA,IA7BA,EA6BA;AACA,aAAA,IAAA,CAAA,MAAA,CAAA,OAAA,CAAA;AACA,KA/BA;AAgCA,IAAA,aAhCA,yBAgCA,OAhCA,EAgCA;AACA,UAAA,KAAA,UAAA,EAAA;AACA,eAAA,KAAA,YAAA;AACA,OAFA,MAEA;AACA,eAAA,KAAA,YAAA,CAAA,MAAA,CAAA,UAAA,CAAA;AAAA,iBAAA,OAAA,CAAA,CAAA,CAAA,CAAA,CAAA,CAAA,GAAA,CAAA;AAAA,SAAA,CAAA;AACA;;AAAA;AACA,KAtCA;AAuCA,IAAA,aAvCA,yBAuCA,KAvCA,EAuCA,IAvCA,EAuCA,GAvCA,EAuCA;AACA,UAAA,CAAA,GAAA,KAAA,YAAA,CAAA,SAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,EAAA,KAAA,GAAA,CAAA,EAAA;AAAA,OAAA,CAAA;AACA,WAAA,YAAA,CAAA,CAAA,EAAA,IAAA,CAAA,CAAA,CAAA,IAAA,KAAA,GAAA,GAAA,GAAA,GAAA,CAAA,kBAAA;AACA,WAAA,YAAA,CAAA,CAAA,EAAA,IAAA,CAAA,CAAA,CAAA,IAAA,KAAA,GAAA,GAAA;AACA,KA3CA;AA4CA,IAAA,aA5CA,yBA4CA,IA5CA,EA4CA,GA5CA,EA4CA;AACA,aAAA,GAAA,CAAA,IAAA,CAAA,CAAA,CAAA,CAAA,GAAA,GAAA;AACA,KA9CA;AA+CA,IAAA,cA/CA,4BA+CA;AACA,UAAA,KAAA,QAAA,EAAA;AACA,YAAA,QAAA,oCAAA,KAAA,QAAA,CAAA,EAAA,MAAA;AACA,aAAA,QAAA,CAAA,QAAA,GAAA,IAAA;AACA,eAAA,QAAA;AACA,OAJA,MAIA;AACA,YAAA,SAAA,4BAAA,KAAA,UAAA,wBAAA,KAAA,QAAA,CAAA,EAAA,MAAA;;AACA,aAAA,QAAA,CAAA,MAAA,GAAA,IAAA;AACA,eAAA,SAAA;AACA;AACA,KAzDA;AA0DA,IAAA,QA1DA,sBA0DA;AAAA;;AACA,UAAA,QAAA,GAAA,KAAA,cAAA,EAAA;AACA,MAAA,UAAA,CAAA,QAAA,EAAA,OAAA,EAAA,KAAA,QAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,iBAAA,EAAA,IAAA;;AACA,QAAA,KAAA,CAAA,kBAAA,GAAA,KAAA;AACA,OAHA;AAIA;AAhEA;AAzDA,CAAA","sourcesContent":["<template lang=\"html\">\r\n  <v-form @submit.prevent=\"onSubmit\">\r\n    <v-card>\r\n      <v-card-title\r\n        v-if=\"!showPlatformDetail || template\"\r\n        @click=\"showPlatformDetail = !showPlatformDetail\">\r\n        {{ customName }}\r\n      </v-card-title>\r\n      <v-text-field v-else\r\n        dense solo hide-details\r\n        :value=\"customName\"\r\n        @change=\"platform.custom_name = $event\"\r\n      />\r\n      <v-card-text\r\n        @click=\"showPlatformDetail = !showPlatformDetail\">\r\n        {{ platform.status }} <br>\r\n        {{ platform.platform_type }}<br>\r\n        PDS Date: {{ platform.PDS_date }} <br>\r\n        Total value: {{ platform.platform_total | toCurrency }}\r\n      </v-card-text>\r\n    </v-card>\r\n    <v-card\r\n      v-if=\"showPlatformDetail\">\r\n      <v-row\r\n        v-for=\"item in displayItemsAdmin\"\r\n        :key=\"item[0].value\"\r\n        dense>\r\n        <v-col\r\n          :cols=\"6\"\r\n          dense\r\n        >\r\n          <v-card\r\n            class=\"pa-2\">\r\n            {{ item[1] }}\r\n          </v-card>\r\n        </v-col>\r\n        <v-col\r\n          :cols=\"6\"\r\n          dense>\r\n          <v-text-field\r\n            v-if=\"item[3] === 'string' || platform[item[3]] == 'number' \"\r\n            dense solo hide-details\r\n            v-model=\"platform[item[0]]\"\r\n          />\r\n          <v-checkbox\r\n            v-if=\"item[3] === 'boolean'\"\r\n            dense solo hide-details\r\n            v-model=\"platform[item[0]]\"\r\n          />\r\n          <!-- Needs to be updated to a date picker -->\r\n          <v-text-field\r\n            v-if=\"item[3] === 'date'\"\r\n            dense solo hide-details\r\n            v-model=\"platform[item[0]]\"\r\n          />\r\n          <v-select\r\n            v-if=\"item[3] === 'list'\"\r\n            :items=\"getReadableList(item)\"\r\n            @change=\"convertToRealName(item, $event)\"\r\n            :value=\"convertLongName(item)\"\r\n            solo dense\r\n            hide-details\r\n          ></v-select>\r\n          <!-- Needs to be updated to a hyperlink -->\r\n          <v-text-field\r\n            v-if=\"item[3] === 'link'\"\r\n            dense solo hide-details\r\n            v-model=\"platform[item[0]]\"\r\n          />\r\n        </v-col>\r\n        </v-row>\r\n        <v-row\r\n          v-for=\"item in displayItemsMisc\"\r\n          :key=\"item[0].value\"\r\n          dense>\r\n          <v-col\r\n            :cols=\"6\"\r\n            dense\r\n          >\r\n            <v-card\r\n              class=\"pa-2\">\r\n              {{ item[1] }}\r\n            </v-card>\r\n          </v-col>\r\n          <v-col\r\n            :cols=\"6\"\r\n            dense>\r\n            <v-text-field\r\n              v-if=\"item[3] === 'string' || item[3] == 'number' \"\r\n              dense solo hide-details\r\n              v-model=\"platform[item[0]]\"\r\n            />\r\n            <v-checkbox\r\n              v-if=\"item[3] === 'boolean'\"\r\n              dense solo hide-details\r\n              v-model=\"platform[item[0]]\"\r\n            />\r\n            <!-- Needs to be updated to a date picker -->\r\n            <v-text-field\r\n              v-if=\"item[3] === 'date'\"\r\n              dense solo hide-details\r\n              v-model=\"platform[item[0]]\"\r\n            />\r\n            <v-select\r\n              v-if=\"item[3] === 'list'\"\r\n              :items=\"getReadableList(item)\"\r\n              @change=\"convertToRealName(item, $event)\"\r\n              :value=\"convertLongName(item)\"\r\n              solo dense\r\n              hide-details\r\n            ></v-select>\r\n            <!-- Needs to be updated to a hyperlink -->\r\n            <v-text-field\r\n              v-if=\"item[3] === 'link'\"\r\n              dense solo hide-details\r\n              v-model=\"platform[item[0]]\"\r\n            />\r\n          </v-col>\r\n          </v-row>\r\n      <v-btn type=\"submit\" color=\"primary\" v-if=\"showPlatformDetail\">\r\n        Save\r\n      </v-btn>\r\n    </v-card>\r\n  </v-form>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service.js\";\r\n\r\nexport default {\r\n  name: \"PlatformDetail\",\r\n  inject: {\r\n    platformId: { default: null },\r\n    scenarioId: { default: null },\r\n  },\r\n  props: {\r\n    platform: {\r\n      type: Object,\r\n      required: true,\r\n    },\r\n    template: {\r\n      type: Boolean,\r\n      required: false,\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      error: null,\r\n      showPlatformDetail: false,\r\n      displayItemsAdmin: [\r\n          //format is [\"field name\", \"friendly name\", \"editable by\", \"type\", \"tooltip\"]\r\n          [\"status\", \"Platform status\",\"all\",\"list\",null],\r\n          [\"platform_type\", \"Platform type\",\"all\",\"list\",null],\r\n          [\"PDS_date\", \"PDS date\",\"admin\",\"date\",null],\r\n          [\"PDS_version\", \"PDS version\",\"admin\",\"string\",null],\r\n          [\"PDS_link\", \"Link to PDS\",\"admin\",\"link\",null],\r\n          [\"AA_link\", \"Link to Asset Allocation\",\"admin\",\"link\",null],\r\n          [\"last_checked\", \"Last checked\",\"admin\",\"date\",null],\r\n          [\"notes\",\"Notes\",\"admin\",\"string\",null]\r\n        ],\r\n      displayItemsMisc: [\r\n          //format is [\"field name\", \"friendly name\", \"editable by\", \"tooltip\"]\r\n          [\"shared_admin_fee\", \"Shared admin fee\",\"all\",\"boolean\",\"If a platform has multiple fee structures, but only one fixed admin fee will be charged\"],\r\n          [\"single_fee_group\", \"Single fee group\",\"all\",\"boolean\",'If there is no \"splitting\" of platform fee groupings. That is, you can only have one fee group for the platform.'],\r\n          [\"allowed_fee_link\", \"Account linking allowed\",\"all\",\"list\",\"The methods allowed to link accounts for the purposes of amin fee splitting.\"],\r\n          [\"maximum_linked_accounts\", \"Number of linked accounts\",\"all\",\"number\",\"Maximum number of accounts that can be linked\"],\r\n          [\"fee_link_type\", \"Fee linking effect\",\"all\",\"list\",\"The way that fee linking modifies the administration fee.\"],\r\n          [\"admin_fee_linking_reduction\", \"Admin fee reduction through linking\",\"all\",\"number\",\"The amount that the admin fee is reduced by linking accounts.\"],\r\n          [\"fee_link_group\", \"Linkable platforms\",\"all\",\"FK\",\"Platforms that can be linked together.\"],\r\n          [\"admin_fee_cutoff\", \"Fee linking effect\",\"all\",\"number\",\"The way that fee linking modifies the administration fee.\"],\r\n          [\"white_label_admin_fee\", \"White labled admin fee\",\"all\",\"boolean\",\"The platform includes the administration fee in the investment fee of its investment options.\"],\r\n          [\"ORR_levy_cap\", \"Maximum ORR Levy\",\"all\",\"number\",\"The cap on the ORR Levy for the entire platform.\"],\r\n        ],\r\n    };\r\n  },\r\n  computed: {\r\n    customName: function (){\r\n      if (this.platform.name == null) {\r\n        return this.platform.custom_name;\r\n      } else if (this.platform.name) {\r\n        return this.platform.name.name;\r\n      } else {\r\n        return null;\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    getReadableList(item){\r\n      let list = this.platform[this.joinList(item[0])];\r\n      let cleanList = [];\r\n      for (let i=0; i<list.length; i++) {\r\n        cleanList.push(list[i][1]);\r\n      }\r\n      return cleanList\r\n    },\r\n    convertLongName(item){\r\n      let list = this.platform[this.joinList(item[0])];\r\n      let realName = \"\";\r\n      for (let i=0; i<list.length; i++) {\r\n        if (this.platform[item[0]] == list[i][0]) {\r\n          realName = list[i][1];\r\n        }\r\n      }\r\n      return realName;\r\n    },\r\n    convertToRealName(item, event){\r\n      let list = this.platform[this.joinList(item[0])];\r\n      let realName = \"\";\r\n      for (let i=0; i<list.length; i++) {\r\n        if (event == list[i][1]) {\r\n          realName = list[i][0];\r\n          this.platform[item[0]] = realName;\r\n        }\r\n      }\r\n    },\r\n    joinList(item){\r\n      return item.concat(\"_list\");\r\n    },\r\n    filteredItems(feeItem){\r\n      if (this.expandFees) {\r\n        return this.displayItems;\r\n      } else {\r\n        return this.displayItems.filter(i => feeItem[i[1]] > 0);\r\n      };\r\n    },\r\n    setPercentage(event, item, fee) {\r\n      const i = this.platformFees.findIndex(item => item.id === fee.id);\r\n      this.platformFees[i][item[1]] = event / 100 * fee.platform_fee_total;\r\n      this.platformFees[i][item[2]] = event / 100;\r\n    },\r\n    getPercentage(item, fee) {\r\n      return (fee[item[2]]*100)\r\n    },\r\n    templateToggle() {\r\n      if (this.template){\r\n        let endpoint = `/api/platformtemplates/${this.platform.id}/`;\r\n        this.platform.template = true;\r\n        return endpoint\r\n      } else {\r\n        let endpoint = `/api/scenarios/${this.scenarioId}/platforms/${this.platform.id}/`;\r\n        this.platform.edited = true;\r\n        return endpoint\r\n      }\r\n    },\r\n    onSubmit() {\r\n      let endpoint = this.templateToggle()\r\n      apiService(endpoint, \"PATCH\", this.platform).then(data => {\r\n        this.$emit(\"modify-platform\", data);\r\n        this.showPlatformDetail = false;\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}