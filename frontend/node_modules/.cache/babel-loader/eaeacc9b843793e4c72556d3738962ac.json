,\r\n      error: null,\r\n      next: null,\r\n\r\n    }\r\n  },\r\n  methods: {\r\n    setPageTitle(title){\r\n      document.title = title;\r\n    },\r\n    getScenarioData() {\r\n      let endpoint = `/api/scenarios/${this.id}/`\r\n      apiService(endpoint)\r\n        .then(data => {\r\n          if (data) {\r\n          this.scenario = data;\r\n          this.setPageTitle(data.client);\r\n        } else {\r\n          this.scenario = null;\r\n          this.setPageTitle(\"404 - Page Not Found\");\r\n        }\r\n        })\r\n    },\r\n    getScenarioPlatforms() {\r\n      let endpoint = `/api/scenarios/${this.id}/platforms/`;\r\n      if (this.next) {\r\n        endpoint = this.next;\r\n      }\r\n      apiService(endpoint)\r\n      .then(data => {\r\n        this.platforms.push(...data.results);\r\n        if (data.next) {\r\n          this.next = data.next;\r\n        } else {\r\n          this.next = null;\r\n        }\r\n      })\r\n    },\r\n    addPlatform(platform) {\r\n        this.platforms.push(platform)\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getScenarioData();\r\n    this.getScenarioPlatforms();\r\n    document.title = \"Feez\";\r\n  },\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.error {\r\n  color: red;\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}