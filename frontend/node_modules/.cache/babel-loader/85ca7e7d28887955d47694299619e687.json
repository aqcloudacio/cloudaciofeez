,SAAA,CAAA,IAAA,CAAA,QAAA;AACA,WAAA,cAAA,CAAA,QAAA;AACA,KArDA;AAsDA,IAAA,SAtDA,qBAsDA,cAtDA,EAsDA;AACA,UAAA,cAAA,CAAA,MAAA,IAAA,SAAA,EAAA;AACA,aAAA,WAAA,CAAA,MAAA,CAAA,KAAA,WAAA,CAAA,OAAA,CAAA,cAAA,CAAA,EAAA,CAAA;AACA,OAFA,MAEA,IAAA,cAAA,CAAA,MAAA,IAAA,aAAA,EAAA;AACA,aAAA,eAAA,CAAA,MAAA,CAAA,KAAA,eAAA,CAAA,OAAA,CAAA,cAAA,CAAA,EAAA,CAAA;AACA,OAFA,MAEA,IAAA,cAAA,CAAA,MAAA,IAAA,aAAA,EAAA;AACA,aAAA,eAAA,CAAA,MAAA,CAAA,KAAA,eAAA,CAAA,OAAA,CAAA,cAAA,CAAA,EAAA,CAAA;AACA,OAFA,MAEA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,cAAA;AACA;;AACA,UAAA,eAAA,GAAA,KAAA,SAAA,CAAA,OAAA,CAAA,cAAA,CAAA;AACA,WAAA,OAAA,CAAA,KAAA,SAAA,EAAA,eAAA;AACA,KAlEA;AAmEA,IAAA,YAnEA,wBAmEA,MAnEA,QAmEA;AAAA,UAAA,KAAA,QAAA,KAAA;;AACA,UAAA,KAAA,IAAA,SAAA,EAAA;AACA,YAAA,eAAA,GAAA,KAAA,SAAA,CAAA,KAAA,SAAA,CAAA,OAAA,CAAA,KAAA,CAAA,OAAA,CAAA,CAAA;AACA,QAAA,eAAA,CAAA,MAAA,GAAA,MAAA;AACA,aAAA,mBAAA,CAAA,eAAA;AACA;AACA,KAzEA;AA0EA,IAAA,aA1EA,yBA0EA,KA1EA,EA0EA;AACA,WAAA,WAAA,CAAA,KAAA;AACA,KA5EA;AA6EA,IAAA,mBA7EA,+BA6EA,QA7EA,EA6EA;AACA,UAAA,QAAA,4BAAA,KAAA,EAAA,wBAAA,QAAA,CAAA,EAAA,MAAA;AACA,MAAA,UAAA,CAAA,QAAA,EAAA,OAAA,EAAA;AAAA,QAAA,MAAA,EAAA,QAAA,CAAA;AAAA,OAAA,CAAA;AACA;AAhFA,GA/CA;AAiIA,EAAA,OAjIA,qBAiIA;AACA,SAAA,eAAA;AACA,SAAA,oBAAA;AACA,IAAA,QAAA,CAAA,KAAA,GAAA,MAAA;AACA;AArIA,CAAA","sourcesContent":["<template>\r\n  <v-container>\r\n    <v-container v-if=\"scenario\">\r\n      <span class=\"display-2 font-weight-bold\">{{ scenario.client }}</span>\r\n       <v-btn :to=\"{ name: 'output', params: { scenarioId:this.id } }\"\r\n              color=\"secondary\"\r\n              justify=\"right\">\r\n                  Results\r\n        </v-btn>\r\n      <v-row>\r\n        <v-col :cols=\"4\">\r\n          <v-row>\r\n            <v-col>\r\n              <Healthcheck\r\n                :currentPlatforms=\"currentList\"\r\n                :recommendedPlatforms=\"recommendedList\"\r\n                :alternativePlatforms=\"alternativeList\"\r\n              />\r\n\r\n            </v-col>\r\n          </v-row>\r\n          <v-row>\r\n              <v-col>\r\n                <FeeChartSummary\r\n                  :currentPlatforms=\"currentList\"\r\n                  :recommendedPlatforms=\"recommendedList\"\r\n                  :alternativePlatforms=\"alternativeList\"\r\n                />\r\n              </v-col>\r\n          </v-row>\r\n        </v-col>\r\n        <v-col :cols=\"8\">\r\n          <AAChartSummary\r\n            :riskProfile=\"riskProfile\"\r\n            :currentPlatforms=\"currentList\"\r\n            :recommendedPlatforms=\"recommendedList\"\r\n            :alternativePlatforms=\"alternativeList\"\r\n          />\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n    <v-container\r\n      class=\"grey lighten-2\">\r\n      <v-row>\r\n        <template\r\n          v-for=\"status in statuses\">\r\n          <v-col\r\n            :key=\"status.index\">\r\n            <v-card>\r\n              <v-card-title\r\n                class=\"justify-center\">\r\n              {{ status }}\r\n              </v-card-title>\r\n            </v-card>\r\n         </v-col>\r\n         <v-divider\r\n            vertical  class=\"mx-0\" :key=\"status.index\"></v-divider>\r\n        </template>\r\n      </v-row>\r\n      <v-row>\r\n        <v-col\r\n          v-for=\"status in statuses\"\r\n          :key=\"status.index\">\r\n          <AddPlatform\r\n            :status=\"status\"\r\n            @add-platform=\"addPlatform\" />\r\n        </v-col>\r\n      </v-row>\r\n      <v-row>\r\n        <v-col>\r\n          <v-row>\r\n            <draggable\r\n              class=\"dragList\"\r\n              v-model=\"currentList\"\r\n              group=\"statusLists\"\r\n              @start=\"drag=true\"\r\n              @end=\"drag=false\"\r\n              @change=\"updateStatus('Current', $event)\"\r\n              :emptyInsertThreshold=\"5\"\r\n              >\r\n              <v-col\r\n                v-for=\"platform in currentList\"\r\n                :key=\"platform.id\">\r\n                <PlatformSummary\r\n                  :scenario=\"scenario\"\r\n                  :platform=\"platform\"\r\n                  @delete-object=\"deleteObj\"\r\n                  @clone-platform=\"clonePlatform\"\r\n                />\r\n              </v-col>\r\n            </draggable>\r\n          </v-row>\r\n        </v-col>\r\n        <v-col>\r\n          <v-row>\r\n            <draggable\r\n              class=\"dragList\"\r\n              v-model=\"recommendedList\"\r\n              group=\"statusLists\"\r\n              @start=\"drag=true\"\r\n              @end=\"drag=false\"\r\n              @change=\"updateStatus('Recommended', $event)\"\r\n              :emptyInsertThreshold=\"5\">\r\n              <v-col\r\n                v-for=\"platform in recommendedList\"\r\n                :key=\"platform.id\">\r\n                      <PlatformSummary\r\n                        :scenario=\"scenario\"\r\n                        :platform=\"platform\"\r\n                        @delete-object=\"deleteObj\"\r\n                        @clone-platform=\"clonePlatform\"\r\n                      />\r\n              </v-col>\r\n            </draggable>\r\n          </v-row>\r\n        </v-col>\r\n        <v-col>\r\n          <v-row>\r\n            <draggable\r\n              class=\"dragList\"\r\n              v-model=\"alternativeList\"\r\n              group=\"statusLists\"\r\n              @start=\"drag=true\"\r\n              @end=\"drag=false\"\r\n              @change=\"updateStatus('Alternative', $event)\"\r\n              :emptyInsertThreshold=\"5\">\r\n              <v-col\r\n                v-for=\"platform in alternativeList\"\r\n                :key=\"platform.id\">\r\n                      <PlatformSummary\r\n                        :scenario=\"scenario\"\r\n                        :platform=\"platform\"\r\n                        @delete-object=\"deleteObj\"\r\n                        @clone-platform=\"clonePlatform\"\r\n\r\n                      />\r\n              </v-col>\r\n            </draggable>\r\n          </v-row>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service.js\";\r\nimport AddPlatform from \"@/components/AddPlatform.vue\";\r\nimport PlatformSummary from \"@/components/PlatformSummary.vue\";\r\nimport Healthcheck from \"@/components/Healthcheck.vue\";\r\nimport FeeChartSummary from \"@/components/FeeChartSummary.vue\";\r\nimport AAChartSummary from \"@/components/AAChartSummary.vue\";\r\nimport draggable from 'vuedraggable'\r\n\r\nexport default {\r\n  name: \"Scenario\",\r\n  provide() {\r\n    return {\r\n      scenarioId: this.id\r\n    };\r\n  },\r\n  props: {\r\n    id: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  components: {\r\n    AddPlatform,\r\n    PlatformSummary,\r\n    draggable,\r\n    Healthcheck,\r\n    FeeChartSummary,\r\n    AAChartSummary,\r\n  },\r\n  data() {\r\n    return {\r\n      scenario: null,\r\n      platforms: [],\r\n      error: null,\r\n      next: null,\r\n      currentList: [],\r\n      recommendedList: [],\r\n      alternativeList: [],\r\n      clone: {},\r\n      statuses: [\r\n        \"Current\",\r\n        \"Recommended\",\r\n        \"Alternative\"\r\n      ],\r\n    };\r\n  },\r\n  computed: {\r\n    riskProfile() {\r\n      if (this.scenario.risk_profile != undefined) {\r\n        return this.scenario.risk_profile;\r\n      } else {\r\n        return {}\r\n      }\r\n    },\r\n  },\r\n  methods: {\r\n    allocateToList(platform) {\r\n      if (platform.status == \"Current\") {\r\n        this.currentList.push(platform);\r\n      } else if (platform.status == \"Recommended\") {\r\n        this.recommendedList.push(platform);\r\n      } else if (platform.status == \"Alternative\") {\r\n        this.alternativeList.push(platform);\r\n      }\r\n    },\r\n    sortPlatforms() {\r\n      this.platforms.forEach((platform) => {\r\n        this.allocateToList(platform);\r\n      })\r\n    },\r\n    checkNull(list) {\r\n      if (list.length > 0) {\r\n        return list;\r\n      }\r\n    },\r\n    setPageTitle(title) {\r\n      document.title = title;\r\n    },\r\n    getScenarioData() {\r\n      let endpoint = `/api/scenarios/${this.id}/`;\r\n      apiService(endpoint).then(data => {\r\n        if (data) {\r\n          this.scenario = data;\r\n          this.setPageTitle(data.client);\r\n        } else {\r\n          this.scenario = null;\r\n          this.setPageTitle(\"404 - Page Not Found\");\r\n        }\r\n      });\r\n    },\r\n    getScenarioPlatforms() {\r\n      let endpoint = `/api/scenarios/${this.id}/platforms/`;\r\n      if (this.next) {\r\n        endpoint = this.next;\r\n      }\r\n      apiService(endpoint).then(data => {\r\n        this.platforms = data.results;\r\n        this.sortPlatforms();\r\n        if (data.next) {\r\n          this.next = data.next;\r\n        } else {\r\n          this.next = null;\r\n        }\r\n      });\r\n    },\r\n    addPlatform(platform) {\r\n      this.platforms.push(platform);\r\n      this.allocateToList(platform);\r\n    },\r\n    deleteObj(objectToDelete) {\r\n      if (objectToDelete.status == \"Current\") {\r\n        this.currentList.splice(this.currentList.indexOf(objectToDelete),1);\r\n      } else if (objectToDelete.status == \"Recommended\") {\r\n        this.recommendedList.splice(this.recommendedList.indexOf(objectToDelete),1);\r\n      } else if (objectToDelete.status == \"Alternative\") {\r\n        this.alternativeList.splice(this.alternativeList.indexOf(objectToDelete),1);\r\n      } else {\r\n        console.log(objectToDelete);\r\n      }\r\n      let deletedPlatform = this.platforms.indexOf(objectToDelete)\r\n      this.$delete(this.platforms, deletedPlatform);\r\n    },\r\n    updateStatus(status, {added}) {\r\n      if (added != undefined) {\r\n        let changedPlatform = this.platforms[this.platforms.indexOf(added.element)];\r\n        changedPlatform.status = status;\r\n        this.patchPlatformStatus(changedPlatform);\r\n      }\r\n    },\r\n    clonePlatform(clone) {\r\n      this.addPlatform(clone);\r\n    },\r\n    patchPlatformStatus(platform) {\r\n      let endpoint = `/api/scenarios/${this.id}/platforms/${platform.id}/`;\r\n      apiService(endpoint, \"PATCH\", {status: platform.status})\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getScenarioData();\r\n    this.getScenarioPlatforms();\r\n    document.title = \"Feez\";\r\n  }\r\n};\r\n</script>\r\n<style media=\"screen\">\r\n  .dragList {\r\n    width: 100%;\r\n    min-height: 300px;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/views"}]}