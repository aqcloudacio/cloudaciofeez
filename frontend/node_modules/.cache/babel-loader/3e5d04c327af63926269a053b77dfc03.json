A,KAAA,cAAA,CAAA,WAAA,IAAA,IAAA,EAAA;AACA,eAAA,KAAA,cAAA,CAAA,WAAA;AACA,OAFA,MAEA,IAAA,KAAA,cAAA,CAAA,IAAA,EAAA;AACA,eAAA,KAAA,cAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAFA,MAEA;AACA,eAAA,IAAA;AACA;AACA;AAZA,IAXA;AAyBA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,UAAA,KAAA,CAAA,CAAA,OAAA,CAAA,KAAA,OAAA,CAAA,EAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,WAAA;AACA;AACA,KALA;AAMA,IAAA,YANA,0BAMA;AAAA;;AACA,UAAA,QAAA,GAAA,iBAAA;AACA,MAAA,UAAA,CAAA,QAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA,CAAA,OAAA;AACA,OAFA;AAGA,KAXA;AAYA,IAAA,MAZA,oBAYA;AACA,UAAA,IAAA,MAAA;;AACA,UAAA,KAAA,MAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA;AACA,KAjBA;AAkBA,IAAA,aAlBA,2BAkBA;AAAA;;AACA,UAAA,CAAA,KAAA,CAAA,CAAA,OAAA,CAAA,KAAA,gBAAA,CAAA,EAAA;AACA,aAAA,KAAA,CAAA,YAAA,CAAA,IAAA;AACA,aAAA,SAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,mBAAA,EAAA,MAAA,CAAA,gBAAA,EADA,CAEA;;;AACA,cAAA,QAAA,4BAAA,MAAA,CAAA,gBAAA,CAAA,EAAA,MAAA;AACA,UAAA,UAAA,CAAA,QAAA,EAAA,OAAA,EAAA,MAAA,CAAA,gBAAA,CAAA,CAJA,CAKA;;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA,UAAA;AAAA,YAAA,MAAA,EAAA;AAAA,cAAA,EAAA,EAAA,MAAA,CAAA,gBAAA,CAAA;AAAA;AAAA,WAAA;;AACA,UAAA,MAAA,CAAA,gBAAA,GAAA,IAAA;AACA,SARA;AASA;AACA,KA/BA;AAgCA,IAAA,UAhCA,wBAgCA;AACA,UAAA,QAAA,GAAA,UAAA;;AACA,UAAA,KAAA,MAAA,CAAA,IAAA,KAAA,QAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,UAAA;AAAA,UAAA,MAAA,EAAA;AAAA,YAAA,EAAA,EAAA,KAAA,cAAA,CAAA;AAAA;AAAA,SAAA;AACA;AACA,KArCA;AAsCA,IAAA,SAtCA,uBAsCA;AACA,UAAA,IAAA,aAAA;;AACA,UAAA,KAAA,MAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA;AACA,KA3CA;AA4CA,IAAA,MA5CA,oBA4CA;AACA,UAAA,QAAA,YAAA;;AACA,UAAA,KAAA,MAAA,CAAA,IAAA,KAAA,QAAA,EAAA;AACA,YAAA,KAAA,cAAA,CAAA,EAAA,IAAA,IAAA,EAAA;AACA,eAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA,SAAA;AAAA,YAAA,MAAA,EAAA;AAAA,cAAA,UAAA,EAAA,KAAA,cAAA,CAAA;AAAA;AAAA,WAAA;AACA;AACA;AACA;AAnDA,GAzBA;AA8EA,EAAA,OA9EA,qBA8EA;AACA,SAAA,YAAA;AACA,SAAA,UAAA;AACA;AAjFA,CAAA","sourcesContent":["<template lang=\"html\">\r\n  <div>\r\n    <v-toolbar color=\"teal lighten-2\" dark dense prominent  class=\"shrink\">\r\n      <v-container style=\"width: 1012px\">\r\n        <v-row>\r\n          <v-col align-self=\"center\" :cols=\"6\">\r\n            <v-toolbar-title large>ProductRex</v-toolbar-title>\r\n          </v-col>\r\n          <v-col :cols=\"3\" align=\"right\" align-self=\"center\">\r\n            <v-autocomplete\r\n              :items=\"scenarios\"\r\n              v-model=\"selectedScenario\"\r\n              no-data-text=\"Client not found\"\r\n              item-text=\"client\"\r\n              return-object\r\n              hide-details\r\n              ref=\"autocomplete\"\r\n              append-icon=\"mdi-magnify\"\r\n              style=\"width: 250px\"\r\n              @change=\"goNewScenario\"/>\r\n          </v-col>\r\n          <v-col align=\"right\" :cols=\"3\">\r\n            <Notifications/>\r\n            <v-btn icon  @click=\"goProfile\">\r\n              <v-icon>mdi-account-outline</v-icon>\r\n            </v-btn>\r\n            <v-btn icon>\r\n              <v-icon>mdi-help-circle-outline</v-icon>\r\n            </v-btn>\r\n            <v-btn icon href=\"/accounts/logout/\">\r\n              <v-icon>mdi-power</v-icon>\r\n            </v-btn>\r\n          </v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </v-toolbar>\r\n    <v-toolbar color=\"teal lighten-4\" short class=\"shrink\">\r\n      <v-container style=\"width: 1012px\">\r\n        <v-row>\r\n          <v-col align-self=\"center\">\r\n            <v-toolbar-title>\r\n              <v-btn icon @click=\"goHome\">\r\n                <v-icon>mdi-home</v-icon>\r\n              </v-btn>\r\n              <template v-if=\"activeScenario.id\">\r\n                <v-icon>mdi-chevron-right</v-icon>\r\n                <span class=\"scenarioLink\" @click=\"goScenario\">  {{activeScenario.client}}</span>\r\n              </template>\r\n              <template v-if=\"activePlatform.id != null\">\r\n                <v-icon>mdi-chevron-right</v-icon>\r\n                <span>  {{customName}}</span>\r\n              </template>\r\n            </v-toolbar-title>\r\n          </v-col>\r\n          <v-col align=\"right\"  v-if=\"activeScenario.id\">\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn icon @click=\"goDocs\">\r\n                <v-icon>mdi-file-document-outline</v-icon>\r\n              </v-btn>\r\n              <ShareScenario\r\n                :scenario=\"activeScenario\"\r\n              />\r\n            </v-card-actions>\r\n        </v-col>\r\n      </v-row>\r\n      </v-container>\r\n    </v-toolbar>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service\";\r\nimport Notifications from \"@/components/Notifications.vue\";\r\nimport { mapGetters } from \"vuex\";\r\nimport { SET_ACTIVE_SCENARIO } from \"@/store/mutations.type\";\r\nimport {\r\n  ADVERTS_ALL\r\n} from \"@/store/actions.type\";\r\nimport ShareScenario from '@/components/ShareScenario.vue';\r\n\r\nexport default {\r\n  components: {\r\n    Notifications,\r\n    ShareScenario,\r\n  },\r\n  data() {\r\n    return {\r\n      scenarios: [],\r\n      selectedScenario: null,\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"activeScenario\", \"activeUser\", \"activePlatform\", \"adverts\"]),\r\n    customName() {\r\n      if (this.activePlatform.name == null) {\r\n        return this.activePlatform.custom_name;\r\n      } else if (this.activePlatform.custom_name != null) {\r\n        return this.activePlatform.custom_name;\r\n      } else if (this.activePlatform.name) {\r\n        return this.activePlatform.name.name;\r\n      } else {\r\n        return null;\r\n      }\r\n    },\r\n  },\r\n  methods: {\r\n    getAdverts() {\r\n      if (this._.isEmpty(this.adverts)) {\r\n        this.$store.dispatch(ADVERTS_ALL)\r\n      }\r\n    },\r\n    getScenarios() {\r\n      let endpoint = \"/api/scenarios/\";\r\n      apiService(endpoint).then(data => {\r\n        this.scenarios = data.results;\r\n      });\r\n    },\r\n    goHome() {\r\n      const path = `/`\r\n      if (this.$route.path !== path)  {\r\n        this.$router.push({ name: 'home' });\r\n      }\r\n    },\r\n    goNewScenario() {\r\n      if (!this._.isEmpty(this.selectedScenario)) {\r\n        this.$refs.autocomplete.blur();\r\n        this.$nextTick(() => {\r\n          this.$store.commit(SET_ACTIVE_SCENARIO, this.selectedScenario);\r\n          // Empty API patch to update the last_edited field of the scenario\r\n          let endpoint = `/api/scenarios/${this.selectedScenario.id}/`;\r\n          apiService(endpoint, \"PATCH\", this.selectedScenario);\r\n          // Nav to the scenario and reset the searchbar\r\n          this.$router.push({ name: 'scenario', params: { id: this.selectedScenario.id } });\r\n          this.selectedScenario = null;\r\n        });\r\n      }\r\n    },\r\n    goScenario() {\r\n      const pathName = 'scenario'\r\n      if (this.$route.name !== pathName) {\r\n        this.$router.push({ name: 'scenario', params: { id: this.activeScenario.id } });\r\n      }\r\n    },\r\n    goProfile() {\r\n      const path = `/profile`\r\n      if (this.$route.path !== path)  {\r\n        this.$router.push({ name: 'profile' });\r\n      }\r\n    },\r\n    goDocs() {\r\n      const pathName = `reports`\r\n      if (this.$route.name !== pathName)  {\r\n        if (this.activeScenario.id != null) {\r\n          this.$router.push({ name: 'reports', params: { scenarioId: this.activeScenario.id } });\r\n        }\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getScenarios();\r\n    this.getAdverts();\r\n  }\r\n}\r\n</script>\r\n<style media=\"screen\">\r\n  .noTransition::after {\r\n  transition: none !important;\r\n  background-color: transparent !important;\r\n  }\r\n  .scenarioLink {\r\n  cursor: pointer;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/components"}]}