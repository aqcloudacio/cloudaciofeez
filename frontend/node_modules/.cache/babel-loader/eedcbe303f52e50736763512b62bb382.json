AA,eAAA,EAAA,CADA;AAEA,MAAA,KAAA,EAAA,IAFA;AAGA,MAAA,IAAA,EAAA,IAHA;AAIA,MAAA,KAAA,EAAA,IAJA;AAKA,MAAA,QAAA,EAAA,IALA;AAMA,MAAA,IAAA,EAAA,EANA;AAOA,MAAA,OAAA,EAAA,IAPA;AAQA,MAAA,MAAA,EAAA,KARA;AASA,MAAA,OAAA,EAAA;AATA,KAAA;AAWA,GA5BA;AA6BA,EAAA,QAAA,oBACA,UAAA,CAAA,CAAA,YAAA,CAAA,CADA,CA7BA;AAgCA,EAAA,OAAA,EAAA;AACA,IAAA,gBADA,4BACA,QADA,EACA;AACA;AACA;AACA;AACA,UAAA,KAAA,UAAA,CAAA,SAAA,IAAA,QAAA,CAAA,YAAA,CAAA,KAAA,IAAA,CAAA,KAAA,MAAA,EAAA;AACA,aAAA,OAAA,GAAA,KAAA;AACA,aAAA,MAAA,GAAA,IAAA;AACA,aAAA,OAAA,GAAA,QAAA;AACA;AACA,KAVA;AAWA,IAAA,kBAXA,gCAWA;AACA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,SAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,QAAA,EAAA;AAAA;AAAA,OAAA;AACA,KAfA;AAgBA,IAAA,iBAhBA,+BAgBA;AACA;AACA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,MAAA,CAAA,KAAA,OAAA,CAAA,EAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,KAtBA;AAuBA,IAAA,WAvBA,yBAuBA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,KA3BA;AA4BA,IAAA,cA5BA,0BA4BA,QA5BA,EA4BA;AACA,WAAA,gBAAA,CAAA,QAAA;;AACA,UAAA,QAAA,IAAA,IAAA,IAAA,KAAA,OAAA,EAAA;AACA,aAAA,iBAAA,CAAA,QAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,UAAA;AAAA,UAAA,MAAA,EAAA;AAAA,YAAA,EAAA,EAAA,QAAA,CAAA;AAAA;AAAA,SAAA;AACA;AACA,KAlCA;AAmCA,IAAA,SAnCA,qBAmCA,QAnCA,EAmCA;AACA,WAAA,iBAAA,CAAA,QAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,SAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,UAAA,EAAA,QAAA,CAAA;AAAA;AAAA,OAAA;AACA,KAtCA;AAuCA,IAAA,iBAvCA,6BAuCA,QAvCA,EAuCA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,mBAAA,EAAA,QAAA,EADA,CAEA;;AACA,UAAA,QAAA,4BAAA,QAAA,CAAA,EAAA,MAAA;AACA,MAAA,UAAA,CAAA,QAAA,EAAA,OAAA,EAAA,QAAA,CAAA;AACA,KA5CA;AA6CA,IAAA,SA7CA,qBA6CA,cA7CA,EA6CA;AACA,WAAA,OAAA,CAAA,KAAA,SAAA,EAAA,KAAA,SAAA,CAAA,OAAA,CAAA,cAAA,CAAA;AACA,KA/CA;AAgDA,IAAA,MAhDA,kBAgDA,EAhDA,EAgDA;AACA;AACA;AACA,UAAA,KAAA,IAAA,CAAA,QAAA,CAAA,EAAA,CAAA,EAAA;AACA,aAAA,IAAA,CAAA,MAAA,CAAA,KAAA,IAAA,CAAA,OAAA,CAAA,EAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA;AACA,KAxDA;AAyDA,IAAA,eAzDA,2BAyDA,QAzDA,EAyDA;AACA,UAAA,GAAA,GAAA,KAAA,SAAA,CAAA,SAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA,KAAA,QAAA,CAAA,EAAA;AAAA,OAAA,CAAA;AACA,WAAA,SAAA,CAAA,GAAA,IAAA,QAAA;AACA,KA5DA;AA6DA,IAAA,QA7DA,oBA6DA,QA7DA,EA6DA;AAAA;;AACA,UAAA,QAAA,CAAA,MAAA,EAAA;AACA,YAAA,QAAA,4BAAA,QAAA,CAAA,EAAA,MAAA;AACA,QAAA,UAAA,CAAA,QAAA,EAAA,OAAA,EAAA,QAAA,CAAA,CACA,IADA,CACA,UAAA,IAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,EAAA;;AACA,UAAA,KAAA,CAAA,eAAA,CAAA,IAAA;;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,SALA;AAMA;AACA;AAvEA;AAhCA,CAAA","sourcesContent":["<template lang=\"html\">\r\n  <v-container\r\n    fluid class=\"grey lighten-5\">\r\n    <v-row\r\n\r\n      dense\r\n      v-for=\"scenario in scenarios.slice(0, scenariosToLoad)\"\r\n      :key=\"scenario.id\">\r\n      <v-col :cols=\"4\"\r\n        class=\"px-0\">\r\n        <v-card\r\n          v-if=\"edit.includes(scenario.id)\"\r\n          height=\"100%\">\r\n          <v-text-field\r\n            v-model=\"scenario.client\"\r\n            dense\r\n            outlined\r\n            hide-details=\"auto\"\r\n            :rules=\"[v => !!v || 'You must enter a client name']\"/>\r\n          <v-select\r\n            :value=\"scenario.risk_profile\"\r\n            @input=\"scenario.risk_profile_id = $event\"\r\n            :items=\"riskProfiles\"\r\n            item-value=\"id\"\r\n            item-text=\"name\"\r\n            dense\r\n            outlined\r\n            hide-details/>\r\n          <v-btn\r\n            type=\"submit\"\r\n            color=\"primary\"\r\n            @click=\"onSubmit(scenario)\">\r\n              Save\r\n          </v-btn>\r\n        </v-card>\r\n        <v-card\r\n          v-else\r\n          height=\"100%\"\r\n          @click=\"selectScenario(scenario)\"\r\n        >\r\n          <v-card-title>\r\n            {{ scenario.client }} <br>\r\n            {{ scenario.scenario_total | toCurrency }} <br>\r\n            {{ scenario.risk_profile == null ? \"\" : scenario.risk_profile_name }}\r\n          </v-card-title>\r\n        </v-card>\r\n      </v-col>\r\n      <v-col :cols=\"6\"\r\n        class=\"px-0\">\r\n        <v-card\r\n          height=\"100%\"\r\n          class=\"pa-4\">\r\n          <v-chip\r\n            v-for=\"platform in scenario.platforms\"\r\n            :key=\"platform[0]\"\r\n            class=\"ma-1\"\r\n            color=\"success\"\r\n            outlined\r\n            :to=\"{ name: 'platform', params: { scenarioId: scenario.id, id: platform[0] } }\">\r\n            {{ platform[1] }}\r\n          </v-chip>\r\n        </v-card>\r\n      </v-col>\r\n      <v-col :cols=\"2\"\r\n        class=\"px-0\">\r\n        <v-card\r\n          height=\"100%\">\r\n          <v-card-actions>\r\n            <v-btn\r\n              icon\r\n              @click=\"goReports(scenario)\">\r\n              <v-icon color=\"teal\">\r\n                mdi-file-document-outline\r\n              </v-icon>\r\n            </v-btn>\r\n            <ShareScenario\r\n              :scenario=\"scenario\"\r\n              @update-scenarios=\"updateScenarios($event)\"\r\n            />\r\n            <v-btn\r\n              icon\r\n              @click=\"doEdit(scenario.id)\">\r\n              <v-icon color=\"teal\">\r\n                mdi-pencil\r\n              </v-icon>\r\n            </v-btn>\r\n            <DeleteObj\r\n              :objectToDelete=\"scenario\"\r\n              :objectType=\"'scenario'\"\r\n              @delete-object=\"deleteObj\"\r\n            />\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n    <v-dialog\r\n      persistent\r\n      max-width=\"700px\"\r\n      v-model=\"dialog\">\r\n      <v-card>\r\n        <v-card-title>\r\n        The risk profile assigned to this scenario is not part of your\r\n        currently <br> active Risk Profiler.\r\n        </v-card-title>\r\n      </v-card>\r\n      <v-btn @click=\"changeRiskProfiler()\">\r\n        Change active Risk Profiler\r\n      </v-btn>\r\n      <v-btn @click=\"changeRiskProfile()\">\r\n        Change Risk Profile for this scenario\r\n      </v-btn>\r\n      <v-btn @click=\"abortChange()\">\r\n        Abort\r\n      </v-btn>\r\n    </v-dialog>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport DeleteObj from \"@/components/DeleteObj.vue\";\r\nimport { apiService } from \"@/common/api.service.js\";\r\nimport ShareScenario from \"@/components/ShareScenario.vue\";\r\nimport {\r\n  SET_ACTIVE_SCENARIO\r\n} from \"@/store/mutations.type\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"ScenarioSummary\",\r\n  props: {\r\n    scenarios: {\r\n      type: Array,\r\n      required: true\r\n    },\r\n    riskProfiles: {\r\n      type: Array,\r\n      required: true,\r\n    },\r\n  },\r\n  components: {\r\n    DeleteObj,\r\n    ShareScenario,\r\n  },\r\n  data() {\r\n    return {\r\n      scenariosToLoad: 5,\r\n      error: null,\r\n      href: null,\r\n      route: null,\r\n      navigate: null,\r\n      edit: [],\r\n      proceed: true,\r\n      dialog: false,\r\n      clicked: null,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"activeUser\"]),\r\n  },\r\n  methods: {\r\n    checkRiskProfile(scenario) {\r\n      // Checks that the risk profile for this scenario is in the active risk\r\n      // profile group. If it isn't prompts the user to change as this causes\r\n      // issues with the asset allocation charts.\r\n      if (this.activeUser.active_rp != scenario.risk_profile.group && !this.dialog){\r\n        this.proceed = false;\r\n        this.dialog = true;\r\n        this.clicked = scenario;\r\n      }\r\n    },\r\n    changeRiskProfiler(){\r\n      // Sends user to profile screen with a shortcut to risk profiles\r\n      this.dialog = false;\r\n      this.$router.push({ name: 'profile', params: { shortcut: 'riskProfile'} });\r\n    },\r\n    changeRiskProfile(){\r\n      // Activates the edit functions for the scenario that has been clicked\r\n      // so that the user can change the risk profile.\r\n      this.dialog = false;\r\n      this.doEdit(this.clicked.id);\r\n      this.clicked = null;\r\n    },\r\n    abortChange() {\r\n      this.dialog = false;\r\n      this.clicked = null;\r\n      this.proceed = true;\r\n    },\r\n    selectScenario(scenario) {\r\n      this.checkRiskProfile(scenario);\r\n      if (scenario != null && this.proceed ) {\r\n        this.setActiveScenario(scenario);\r\n        this.$router.push({ name: 'scenario', params: { id: scenario.id } })\r\n      }\r\n    },\r\n    goReports(scenario) {\r\n      this.setActiveScenario(scenario);\r\n      this.$router.push({ name: 'reports', params: { scenarioId: scenario.id } })\r\n    },\r\n    setActiveScenario(scenario){\r\n      this.$store.commit(SET_ACTIVE_SCENARIO, scenario);\r\n      // Empty API patch to update the last_edited field of the scenario\r\n      let endpoint = `/api/scenarios/${scenario.id}/`;\r\n      apiService(endpoint, \"PATCH\", scenario);\r\n    },\r\n    deleteObj(objectToDelete) {\r\n      this.$delete(this.scenarios, this.scenarios.indexOf(objectToDelete));\r\n    },\r\n    doEdit(id){\r\n      // Adds the clicked scenario to the edit list (so they can edit multiple)\r\n      // scenarios at once\r\n      if (this.edit.includes(id)) {\r\n        this.edit.splice(this.edit.indexOf(id));\r\n      } else {\r\n      this.edit.push(id);\r\n      }\r\n    },\r\n    updateScenarios(scenario) {\r\n      let idx = (this.scenarios.findIndex(i => i.id === scenario.id));\r\n      this.scenarios[idx] = scenario;\r\n    },\r\n    onSubmit(scenario){\r\n      if (scenario.client) {\r\n        let endpoint = `/api/scenarios/${scenario.id}/`;\r\n        apiService(endpoint, \"PATCH\", scenario\r\n          ).then(data => {\r\n          this.doEdit(scenario.id);\r\n          this.updateScenarios(data);\r\n          this.proceed = true;\r\n        });\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}