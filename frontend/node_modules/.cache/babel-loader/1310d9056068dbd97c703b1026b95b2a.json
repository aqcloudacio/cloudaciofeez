CAAA,KAAA,CAAA,cAAA,EAAA,IAAA;AACA,SAJA;;AAKA,YAAA,KAAA,KAAA,EAAA;AACA,eAAA,KAAA,GAAA,IAAA;AACA;AACA,OAVA,MAUA;AACA,aAAA,KAAA,GAAA,4BAAA;AACA;AACA;AAvBA,GAfA;AAwCA,EAAA,OAxCA,qBAwCA;AACA,SAAA,eAAA;AACA;AA1CA,CAAA","sourcesContent":["<template lang=\"html\">\r\n  <div>\r\n    <form class=\"card\"\r\n          @submit.prevent=\"onSubmit\">\r\n      <div class=\"card-header px-3\">\r\n        Add a platform\r\n      </div>\r\n      <div class=\"card-block\">\r\n        <select class=\"form-control\" v-model=\"newPlatform\">\r\n          <option v-for=\"platform in platformList\"\r\n                  :key=\"platform.id\"\r\n                  :value=\"platform\">\r\n                  {{ platform.name }}\r\n          </option>\r\n        </select>\r\n      </div>\r\n      {{error}}\r\n      <div class=\"card-footer px-3\">\r\n        <button type=\"submit\" class=\"btn btn-sm btn-success\">Add</button>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service.js\";\r\nexport default {\r\n  name: \"PlatformSelector\",\r\n  props: {\r\n    scenario: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      newPlatform: {},\r\n      platformList: [],\r\n      error: null\r\n    }\r\n  },\r\n  methods: {\r\n    getPlatformList() {\r\n      let endpoint = `/api/platformnames/`;\r\n      apiService(endpoint, \"GET\")\r\n        .then(data => {\r\n          this.platformList.push(...data.results)\r\n        })\r\n    },\r\n    onSubmit() {\r\n      // Tell the REST API to create a new answer for this question based on the user input, then update some data properties\r\n      if (this.newPlatform) {\r\n        let endpoint = `/api/scenarios/${this.scenario.id}/platforms/`;\r\n        apiService(endpoint, \"POST\", { name_id: this.newPlatform.id,\r\n                                       scenario: this.scenario.id})\r\n        .then(data => {\r\n          this.$emit(\"add-platform\", data)\r\n        })\r\n        if (this.error) {\r\n          this.error = null;\r\n        }\r\n      } else {\r\n        this.error = \"You must select a platform\";\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getPlatformList();\r\n  }\r\n}\r\n\r\n</script>\r\n"],"sourceRoot":"src/components"}]}