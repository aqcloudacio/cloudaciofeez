AAA,KAAA,EAAA;AACA,aAAA,KAAA,GAAA,iDAAA;AACA,OAFA,MAEA;AACA,aAAA,OAAA,GAAA,IAAA;AACA,aAAA,YAAA;AACA,YAAA,QAAA,4BAAA,KAAA,cAAA,CAAA,EAAA,cAAA;AACA,QAAA,UAAA,CAAA,QAAA,EAAA,MAAA,EAAA;AAAA,UAAA,QAAA,EAAA,KAAA,cAAA,CAAA;AAAA,SAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA,IAAA;;AACA,UAAA,aAAA,CAAA,MAAA,CAAA,KAAA,CAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,SAJA,EAIA,KAJA,CAIA,YAAA;AACA,UAAA,MAAA,CAAA,KAAA,GAAA,sCAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,SAPA;AAQA;AACA,KA1DA;AA2DA,IAAA,iBA3DA,+BA2DA;AAAA;;AACA,UAAA,KAAA,CAAA,CAAA,OAAA,CAAA,KAAA,cAAA,KAAA,KAAA,UAAA,EAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,cAAA,EAAA,KAAA,UAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,aAAA;AACA,SAFA;AAGA,OAJA,MAIA;AACA,aAAA,aAAA;AACA;AACA;AAnEA,GA5DA;AAiIA,EAAA,OAjIA,qBAiIA;AACA,SAAA,iBAAA;AACA,GAnIA;AAoIA,EAAA,gBApIA,4BAoIA,EApIA,EAoIA,IApIA,EAoIA,IApIA,EAoIA;AACA,QAAA,EAAA,CAAA,IAAA,IAAA,UAAA,EAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,qBAAA;AACA;;AACA,IAAA,IAAA;AACA;AAzIA,CAAA","sourcesContent":["<template lang=\"html\">\r\n  <v-container class=\"fullwidthrow pa-0 grey1\" style=\"height:100%\">\r\n    <v-container style=\"width:1012px\">\r\n      <v-row>\r\n        <v-col>\r\n          <v-card class=\"pa-6 rounded-card\">\r\n            <v-row dense>\r\n              <v-col :cols=\"8\">\r\n                <v-row class=\"text-center\">\r\n                  <v-col>\r\n                    <v-btn x-large rounded color=\"primary\" @click=\"getReport\" class=\"pl-4 pa-2 mt-2\">\r\n                      Get ProductRex<v-btn class=\"ml-4\" small fab depressed dark color=\"rgb(48,133,89)\" @click=\"getReport\"><v-icon>mdi-download</v-icon></v-btn>\r\n                    </v-btn>\r\n\r\n                  </v-col>\r\n                </v-row>\r\n                <span color=\"danger\" v-if=\"error\">\r\n                  {{error}}\r\n                </span>\r\n                <v-row>\r\n                  <v-col>\r\n                    <v-container class=\"pl-0 pt-8 pb-6 pr-4\" v-if=\"sponsoredItem\">\r\n                      <v-img class=\"half-rounded-card\" :src=\"sponsoredItem.image\"></v-img>\r\n                      <v-card-subtitle style=\"font-size: 0.8em\" class=\"pa-0\" align=\"end\">\r\n                        <em>Sponsored</em>\r\n                      </v-card-subtitle>\r\n                    </v-container>\r\n                  </v-col>\r\n                </v-row>\r\n                <v-row>\r\n                  <v-col>\r\n                    <v-select\r\n                      class=\"rounded-card pr-4 small-text\"\r\n                      :value=\"activeScenario.theme\"\r\n                      @change=\"changeTheme($event)\"\r\n                      :items=\"themes\"\r\n                      item-text=\"name\"\r\n                      item-value=\"id\"\r\n                      label=\"Theme\"\r\n                      outlined hide-details dense>\r\n                      <template v-slot:append-outer>\r\n                        <v-icon\r\n                          v-if=\"themeChanged\"\r\n                          color=\"green\" icon\r\n                          @click=\"saveTheme()\">mdi-check-circle</v-icon>\r\n                        <v-icon\r\n                          v-if=\"!themeChanged\"\r\n                          color=\"green\" icon\r\n                          @click=\"toggleThemeSettings()\">mdi-wrench</v-icon>\r\n                      </template>\r\n                    </v-select>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-col>\r\n              <!-- <v-divider vertical /> -->\r\n              <template v-if=\"reports\">\r\n                <v-col :cols=\"4\">\r\n                  <v-card class=\"half-rounded-card grey2 pa-6 pt-4\" flat>\r\n                    <v-list dense class=\"py-1 overflow-y-auto transparent\" style=\"max-height:315px\">\r\n                      <v-list-item\r\n                        class=\"px-0\"\r\n                        v-if=\"loading\"\r\n                        dense\r\n                        two-line>\r\n                        <v-list-item-content>\r\n                          <v-list-item-title>{{ loadingText }}\r\n                          </v-list-item-title>\r\n                        </v-list-item-content>\r\n                      </v-list-item>\r\n                      <v-divider v-if=\"loading\"></v-divider>\r\n                      <!-- <template v-for=\"(report, index) in reports.slice(0, reportsToLoad)\"> -->\r\n                      <template v-for=\"report in reports.slice(0, reportsToLoad)\">\r\n                        <v-list-item\r\n                          dense\r\n                          two-line\r\n                          :key=\"report.id\"\r\n                          :href=\"report.file\">\r\n                          <v-list-item-content>\r\n                            <v-list-item-title>ProductRex for {{activeScenario.client}}\r\n                            </v-list-item-title>\r\n                            <v-list-item-subtitle style=\"font-size: 0.75em\">Created: {{report.created_at}}\r\n                            </v-list-item-subtitle>\r\n                          </v-list-item-content>\r\n                        </v-list-item>\r\n                        <!-- <v-divider\r\n                          v-if=\"index != reports.length-1 && index != reportsToLoad-1\"\r\n                          :key=\"index\">\r\n                        </v-divider> -->\r\n                      </template>\r\n                    </v-list>\r\n                    <v-btn\r\n                      style=\"width:100%\"\r\n                      rounded\r\n                      small\r\n                      class=\"grey1\"\r\n                      elevation=\"0\"\r\n                      @click=\"reportsToLoad += 5\"\r\n                      v-if=\"reports.length > 5 && reportsToLoad < reports.length\">\r\n                      View More\r\n                    </v-btn>\r\n                  </v-card>\r\n                </v-col>\r\n              </template>\r\n            </v-row>\r\n            <v-divider\r\n              class=\"mb-4 mt-8\"\r\n              v-show=\"activeScenario && showThemeSettings && !themeChanged\"/>\r\n            <v-row v-show=\"activeScenario && showThemeSettings && !themeChanged\">\r\n              <v-col>\r\n                <ThemeTable\r\n                  v-if=\"activeScenario\"\r\n                  :user=\"activeUser\"\r\n                  :scenario=\"activeScenario\"\r\n                  @load-themes=\"loadThemes($event)\"/>\r\n              </v-col>\r\n            </v-row>\r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { apiService } from \"@/common/api.service.js\";\r\nimport ThemeTable from \"@/components/ThemeTable.vue\";\r\nimport { mapGetters } from \"vuex\";\r\nimport {\r\n  SCENARIO_UPDATE_PROP,\r\n  SCENARIO_FETCH,\r\n } from \"@/store/actions.type\";\r\n import {\r\n   CLEAR_ACTIVE_SCENARIO,\r\n } from \"@/store/mutations.type\";\r\n\r\nexport default {\r\n  name: \"Output\",\r\n  props: {\r\n    scenarioId: {\r\n      type: String,\r\n      required: true,\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      reportsToLoad: 5,\r\n      error: null,\r\n      reports: null,\r\n      themes: [],\r\n      showThemeSettings: false,\r\n      newTheme: null,\r\n      loading: false,\r\n      timer: null,\r\n      loadingText: 'Creating Report',\r\n      text: [\r\n        \"Reticulating Splines\",\r\n        \"Adjusting Bell Curves\",\r\n        \"Aligning Covariance Matrices\",\r\n        \"Asserting Packed Exemplars\",\r\n        \"Growing Data Trees\",\r\n        \"Attemping to Lock Back-Buffer\",\r\n        \"Compounding Inert Tessellations\",\r\n        \"Computing Optimal Bin Packing\",\r\n        \"Containing Existential Buffer\",\r\n        \"Decomposing Singularities\",\r\n        \"Extracting Resources\",\r\n        \"Integrating Curves\",\r\n        \"Iterating Cellular Automata\",\r\n        \"Lecturing Errant Subsystems\",\r\n        \"Obfuscating Quigley Matrix\",\r\n        \"Realigning Alternate Timeframes\",\r\n        \"Reverse Engineering Image Consultant\",\r\n        \"Sequencing Particles\",\r\n        \"Time-Compressing Simulator Clock\",\r\n      ]\r\n    };\r\n  },\r\n  components: {\r\n    ThemeTable,\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"activeScenario\", \"activeUser\", \"adverts\"]),\r\n    themeChanged() {\r\n      if (this.newTheme === null) {\r\n        return false\r\n      } else {\r\n        return !(this.newTheme === this.activeScenario.theme)\r\n      }\r\n    },\r\n    sponsoredItem() {\r\n      let item = [];\r\n      item = this.adverts.filter(x => x.type_name === \"document_cta\");\r\n      return item[0]\r\n    },\r\n  },\r\n  methods: {\r\n    generateText() {\r\n      let _self = this;\r\n      this.timer = setInterval(function() {\r\n        let max = _self.text.length;\r\n        let i = Math.floor(Math.random() * Math.floor(max));\r\n        _self.loadingText =  _self.text[i];\r\n      }, 1500);\r\n    },\r\n    changeTheme(event) {\r\n      this.newTheme = event;\r\n    },\r\n    toggleThemeSettings(){\r\n      this.showThemeSettings = !this.showThemeSettings;\r\n    },\r\n    loadThemes(event){\r\n      this.themes = event;\r\n    },\r\n    saveTheme(){\r\n      if (this.showThemeSettings) {\r\n        this.toggleThemeSettings();\r\n      }\r\n      if (this.newTheme != this.activeScenario.theme) {\r\n        this.$store.dispatch(SCENARIO_UPDATE_PROP, {\r\n          'prop': 'theme',\r\n          'value': this.newTheme\r\n        }).then(() => {\r\n          this.newTheme = null\r\n        })\r\n      }\r\n    },\r\n    getAllReports() {\r\n      let endpoint = `/api/scenarios/${this.activeScenario.id}/reports/`;\r\n      apiService(endpoint).then(data => {\r\n        this.reports = data.results;\r\n        clearInterval(this.timer);\r\n        this.loading = false;\r\n      });\r\n    },\r\n    getReport() {\r\n      if (this.error) {\r\n        this.error = null;\r\n      }\r\n      if (!this.activeScenario.theme) {\r\n        this.error = \"You must select a Theme from the dropdown below\"\r\n      } else {\r\n        this.loading = true;\r\n        this.generateText();\r\n        let endpoint = `/api/scenarios/${this.activeScenario.id}/reports/`;\r\n        apiService(endpoint, \"POST\", {scenario: this.activeScenario.id}).then(data => {\r\n          this.reports.unshift(data)\r\n          clearInterval(this.timer);\r\n          this.loading = false;\r\n        }).catch(() => {\r\n          this.error = \"The document could not be generated.\";\r\n          this.loading = false;\r\n        })\r\n      }\r\n    },\r\n    setActiveScenario(){\r\n      if (this._.isEmpty(this.activeScenario) && this.scenarioId) {\r\n        this.$store.dispatch(SCENARIO_FETCH, this.scenarioId).then(() => {\r\n          this.getAllReports();\r\n        });\r\n      } else {\r\n        this.getAllReports()\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.setActiveScenario();\r\n  },\r\n  beforeRouteLeave(to, from, next) {\r\n    if (to.name != \"scenario\") {\r\n      this.$store.commit(CLEAR_ACTIVE_SCENARIO);\r\n    }\r\n    next()\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.v-list-item {\r\n  padding:0\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}